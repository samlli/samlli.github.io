angular.module('dukemedicine')
    .factory('FetchDoctorService', ['$q', '$http', function($q, $http) {
        var doctorProfile = {};

        // Hashbangs (#!) and everything after get removed by the time
        // the requuest gets to the request handler on the server.
        // So we will split up the url and  send the separate pieces as patams.
        // path is portion before the hashbang.
        // alias is the portion after the hashbang.
        // example: send-card/#!/duke-university-hospital
        //          path=send-card
        //          alias=duke-university-hospital


        var fetchDoctorByEpicId = function(id) {
            var defer = $q.defer();

            if (id !== undefined && id > 0) {
                $http.get('/fetch-doctor-node-by-epic-id?id=' + id)
                    .success(function(data) {
                        if(data.error === undefined) {
                            defer.resolve(data);
                        } else{
                            defer.reject(data);
                        }
                    }).
                    error(function(data) {
                        defer.reject(data);
                    });
                return defer.promise;
            }
        };

        var fetchDoctorProfileByPath = function(alias) {
          var drupalPath = 'find-doctors-physicians';
            var defer = $q.defer();
            var service = this;


            if (alias !== undefined) {
              drupalPath +='&alias=' + alias;
            }
                $http.get('/fetch-node?path=' + drupalPath)
                    .success(function(data) {
                        if(data.error === undefined) {
                          var doctor = {};

                          //hasLocations
                          if(data.field_reference_locations !== null
                            && data.field_reference_locations !== undefined
                            && data.field_reference_locations.length > 0)
                            doctor["hasLocations"] = true;
                        	else
                            doctor["hasLocations"] = false;


                          //epicId
                          doctor["epicId"] = data.field_epic_provider_id;
                          if(doctor["epicId"] == undefined)
                            doctor["epicId"] = null;

                          //nodeID
                          doctor["nid"] = data.nid;

                          //hasOpenScheduling
                          doctor["hasOpenScheduling"] = data.field_has_open_scheduling;
                          if(doctor["hasOpenScheduling"] == undefined)
                            doctor["hasOpenScheduling"] = null;

                           //hasProviderId
                           if(doctor["epicId"]!==null)
                            doctor["hasProviderId"]=true;
                           else
                            doctor["hasProviderId"]=false;

                          //officePhone
                          doctor["officePhone"] = data.field_office_phone;
                          if(doctor["officePhone"] == undefined)
                            doctor["officePhone"] = null;

                          //showOfficePhone
                          doctor["showOfficePhone"] = data.field_hide_office_phone;
                          if(doctor["showOfficePhone"] == undefined || doctor["showOfficePhone"] == null)
                              doctor["showOfficePhone"] = true;
                          else
                              doctor["showOfficePhone"] = false;

                          //dukeId
                          doctor["dukeId"] = data.field_duke_id;
                          if(doctor["dukeId"] == undefined)
                                doctor["dukeId"] = null;


                          //hasFax
                          if (data.field_fax_num !== null && data.field_fax_num !== undefined) {
                              doctor["hasFax"] = true;
                          } else if(doctor["hasLocations"] && data.field_reference_locations[0].field_fax_num.length > 0) {
                            doctor["hasFax"] = true;
                    			} else
                            doctor["hasFax"] = false;

                          //fax
                    		 if (data.field_fax_num !== null && data.field_fax_num !== undefined) {
                            doctor["fax"] = data.field_fax_num;
                    			} else if(doctor["hasLocations"] && data.field_reference_locations[0].field_fax_num.length > 0) {
                            doctor["fax"] = data.field_reference_locations[0].field_fax_num.und[0].safe_value;
                    			} else
                            doctor["fax"] = null;

                          //firstName
                          doctor["firstName"] = data.field_physician_first_name;
                          if(doctor["firstName"] == undefined)
                                doctor["firstName"] = null;

                          //hidePubMedLink
                          doctor["hidePubMedLink"] = data.field_hide_pubmed_link;
                          if(doctor["hidePubMedLink"] == undefined)
                                doctor["hidePubMedLink"] = null;

                          //middleName
                          doctor["middleName"] = data.field_physician_middle_name;
                          if(doctor["middleName"] == undefined)
                              doctor["middleName"] = null;

                          //lastName
                          doctor["lastName"] = data.field_physician_last_name;
                          if(doctor["lastName"] == undefined)
                              doctor["lastName"] = null;

                          //lastName
                          doctor["suffix"] = data.field_physician_suffix;
                          if(doctor["suffix"] == undefined)
                              doctor["suffix"] = null;

                          //lastName
                          doctor["hasDegrees"] = data.field_physician_degrees;
                          if(doctor["hasDegrees"] ==  null || doctor["hasDegrees"] == undefined )
                                doctor["hasDegrees"] = false;
                          else
                            doctor["hasDegrees"] = true;


                          //degrees
                          doctor["degrees"] = data.field_physician_degrees;
                          if(doctor["degrees"] == undefined)
                              doctor["degrees"] = null;

                          //nodeId
                          doctor["nodeId"] = data.nid;
                          if(doctor["nodeId"] == undefined)
                                doctor["nodeId"] = null;

                          //officePhone
                          doctor["nodeId"] = data.field_office_phone;
                          if(doctor["nodeId"] == undefined)
                                doctor["nodeId"] = null;

                          //title
                          doctor["title"] = data.title;
                          if(doctor["title"] == undefined)
                                doctor["title"] = null;

                          //hasSpecialties
                                if (data.field_specialties !== null && data.field_specialties !== undefined && data.field_specialties.length > 0) {
                                    doctor["hasSpecialties"] = true;
                            			}
                                else
                                  doctor["hasSpecialties"] = false;




                          //specialties
                               var specialResults = [];

                               if (data.field_specialties !== null && data.field_specialties !== undefined && data.field_specialties.length > 0) {
                                  for (var i = 0; i < data.field_specialties.length; i++) {
                                    specialResults.push(data.field_specialties[i]);
                                  }
                                  doctor["specialties"]  = specialResults;
                               } else {
                                 doctor["specialties"]  =  null;
                            	 }

                        //initLocations
                               if (data.field_reference_locations !== null && data.field_reference_locations !== undefined && data.field_reference_locations.length > 0) {
                                 doctor["locations"] = []; //$scope.node.field_reference_locations;


                                 var latitude = null;
                                 var longitude = null;

                                 for (var i = 0; i < data.field_reference_locations.length; i++) {
                                   var location = data.field_reference_locations[i];

                                               // Get the phone number
                                   var phone = null;
                                   if (location.field_office_phone !== null && location.field_office_phone !== undefined) {
                                     phone = location.field_office_phone.und[0].safe_value;
                                   }else if (location.field_patient_resources !== null && location.field_patient_resources !== undefined) {
                                     phone = location.field_patient_resources.und[0].url;
                                   }

                                               // Get lat/long
                                   latitude = location.field_location_address.und[0].latitude;
                                   longitude = location.field_location_address.und[0].longitude;

                                               // full address
                                   var fullAddress = [
                                     location.field_location_address.und[0].street,
                                     location.field_location_address.und[0].city,
                                     location.field_location_address.und[0].province,
                                     location.field_location_address.und[0].postal_code,
                                   ].join(', ');

                                               //directions
                                   var directions = 'https://maps.google.com/?s=' + encodeURIComponent(fullAddress);

                                   var result = {
                                       'title': location.title,
                                       'address': location.field_location_address.und[0].street,
                                       'city': location.field_location_address.und[0].city,
                                       'state': location.field_location_address.und[0].province,
                                       'zip': location.field_location_address.und[0].postal_code,
                                       'fullAddress': fullAddress,
                                       'latitude': latitude,
                                       'longitude': longitude,
                                       'phone': phone,
                                       'url': data.location_urls[location.nid],
                                       'directions': directions
                                     };

                                     doctor["locations"].push(result);


                                 }

                               }
                               else
                                 doctor["locations"] = null;


                          //seoKeywordsAdditional
                          doctor["seoKeywordsAdditional"] = data.field_seo_keywords_additional;
                          if(doctor["seoKeywordsAdditional"] == undefined)
                                doctor["seoKeywordsAdditional"] = null;

                          var defaultSeoMetaDescription = function(doctor){
                            			//var defaultString = "As a world-class academic and health care system, Duke Medicine strives to transform medicine and health locally and globally.";

                            			var metaDesc;
                            			metaDesc = doctor["title"];

                            			if(doctor["hasSpecialties"])
                            			{
                            				var specials = doctor["specialties"];
                            				metaDesc += " is a"
                            				for (var i = 0; i < specials.length; i++) {
                            					if(i>0)
                            						metaDesc += " and a";
                            					metaDesc += " "+specials[i];
                            				}
                            				metaDesc += " at Duke Medicine, " ;//" who sees patients at"
                            			}





                                  			if(doctor["hasLocations"])
                                  				{
                                  					var locs = doctor["locations"];
                                  					metaDesc += " who sees patients at "
                                  					for (var i = 0; i < locs.length; i++) {
                                  					   var tempLoc = locs[i];
                                  						if(i>0)
                                  							metaDesc += " and";
                                  						metaDesc += " "+ tempLoc.title + " - " + tempLoc.address + " " + tempLoc.city + ", " + tempLoc.state;

                                  					}
                                  					metaDesc += ".";
                                  				}

                                          //apointmentPhone
                                          if (data.field_appoint_phone !== null && data.field_appoint_phone !== undefined)
                                            doctor["appointmentPhone"] = data.field_appoint_phone;
                                          else if(doctor["hasLocations"]
                                            && data.field_reference_locations[0].field_office_phone.und.length > 0)
                                              doctor["appointmentPhone"] = data.field_reference_locations[0].field_office_phone.und[0].safe_value;
                                          else
                                            doctor["appointmentPhone"] = null;




                                  /*
                                  			if($scope.hasDegrees())
                                  				{
                                  					var degs = $scope.getDegrees();
                                  					metaDesc += " My degrees are"
                                  					for (var i = 0; i < degs.length; i++) {
                                  						metaDesc += " "+degs[i];
                                  					}
                                  					metaDesc += ".";
                                  				}

                                  			*/
                                  			/*
                                  		if($scope.hasBoardCerts())
                                  			{
                                  				var certs = $scope.getBoardCerts();
                                  				metaDesc += " My board certifications are"
                                  				for (var i = 0; i < certs.length; i++) {
                                  					metaDesc += " "+certs[i];
                                  				}
                                  				metaDesc += ".";
                                  			}
                                  			*/
                                  			metaDesc += " Read more about this provider at DukeMedicine.org.";

                                  			return metaDesc;

                                  		}

                          //seoMetaDescription
                          doctor["seoMetaDescription"] = data.field_seo_keywords_additional;
                          if(doctor["seoMetaDescription"] == null || doctor["seoMetaDescription"] == undefined)
                                doctor["seoMetaDescription"] = defaultSeoMetaDescription(doctor);



                          //treatmentTerms
                          var termResults = [];
                          var term = null;

                          if (data.field_treatment_terms !== null && data.field_treatment_terms !== undefined && data.field_treatment_terms.length > 0) {
                            for (var i = 0; i < data.field_treatment_terms.length; i++) {
                              term = data.field_treatment_terms[i];
                              if (term.field_is_hidden.und === undefined || term.field_is_hidden.und[0].value === 0) {
                                termResults.push(term);
                              }
                            }
                            doctor["treatmentTerms"] = termResults;
                          } else {
                            doctor["treatmentTerms"] = null;
                    			}



                       //videoLink
                       if(data.field_doctor_video_link == null || data.field_doctor_video_link.url == null)
                             doctor["videoLink"] = null;
                       else
                            doctor["videoLink"] = data.field_doctor_video_link.url;

                      //hasVideoLink
                      if(doctor["videoLink"] == null || doctor["videoLink"] == undefined)
                            doctor["hasVideoLink"] = false;
                      else
                            doctor["hasVideoLink"] = true;

                      //overview
                      doctor["overview"] = data.field_physician_overview;
                      if(doctor["overview"] == undefined)
                          doctor["overview"] = null;

                      //hasOverview
                      if(doctor["overview"] == null || doctor["overview"] == undefined)
                            doctor["hasOverview"] = false;
                      else
                            doctor["hasOverview"] = true;


                      //hasSpokenLanguages
                      if(data.field_languages_spoken !== undefined && data.field_languages_spoken !== null && data.field_languages_spoken.length > 0)
                        doctor["hasSpokenLanguages"] = true;
                      else
                        doctor["hasSpokenLanguages"] = false;

                      //SpokenLanguages
                      if(doctor["hasSpokenLanguages"])
                        doctor["spokenLanguages"] = data.field_languages_spoken.join(', ');
                      else
                        doctor["spokenLanguages"] = null;


                      //hasAreasOfFocus
                      doctor["hasAreasOfFocus"] = data.field_custom_terms;


                      //areasOfFocus
                      if(doctor["hasAreasOfFocus"])
                        doctor["areasOfFocus"] = data.field_custom_terms.join('.  ');
                      else
                      doctor["areasOfFocus"] = null;

                      //hasPatientStories
                      if(data.related_items !== undefined &&
                        data.related_items.patient_experience !== undefined &&
                        data.related_items.patient_experience !== null &&
                        data.related_items.patient_experience.length > 0)
                        doctor["hasPatientStories"] =  true;
                      else
                        doctor["hasPatientStories"] =  false;

                      //hasRelatedArticles
                      if(data.related_items !== undefined &&
                        data.related_items.blog_post !== undefined &&
                        data.related_items.blog_post !== null &&
                        data.related_items.blog_post.length > 0)
                        doctor["hasRelatedArticles"] =  true;
                      else
                        doctor["hasRelatedArticles"] =  false;

                      //hasIndustryRelations
                      if(data.field_industry_relations !== null &&
                        data.field_industry_relations !== undefined &&
                        data.field_industry_relations.length > 0)
                          doctor["hasIndustryRelations"] =  true;
                      else
                          doctor["hasIndustryRelations"] =  false;

                      //getIndustryRelations
                      var relations = data.field_industry_relations;
                      doctor["industryRelations"] =  relations.filter(function(elem, pos) {
                                        return relations.indexOf(elem) === pos;
                                      });

                      //industryRelationsMessage
                      if(doctor["hasIndustryRelations"]){
                        doctor["industryRelationsMessage"] = 'This faculty member (or a member of their immediate family) ' +
                                    'has a working relationship (i.e. consulting, research, and/or educational services) '+
                                    'with the companies listed below. These relations have been reported ' +
                                    'to the health system leadership and, when appropriate, ' +
                                    'management plans are in place to address potential conflicts. ';
                      }
                      else{
                          doctor["industryRelationsMessage"] =  'This faculty member has no reported relationships with industry.';
                      }

                      //hasTraining
                      if(data.field_physician_training !== null &&
                        data.field_physician_training !== undefined &&
                        data.field_physician_training.length > 0)
                         doctor["hasTraining"] =  true;
                      else
                        doctor["hasTraining"] =  false;

                      //getTraining
                      doctor["training"] = data.field_physician_training;
                      if(doctor["training"] == undefined)
                          doctor["training"] = null;

                      //hasResid
                      if(data.field_physician_residency !== null &&
                        data.field_physician_residency !== undefined &&
                        data.field_physician_residency.length > 0 )
                        doctor["hasResid"] =  true;
                      else
                        doctor["hasResid"] =  false;

                      //getResid
                      doctor["resid"] = data.field_physician_residency;
                      if(doctor["resid"] == undefined)
                          doctor["resid"] = null;

                      //hasFellowship
                      if(data.field_physician_fellowship !== null &&
                         data.field_physician_fellowship !== undefined &&
                         data.field_physician_fellowship.length > 0){
                           doctor["hasFellowship"] =  true;
                      }
                      else
                      doctor["hasFellowship"] =  false;

                      //fellowship
                      if( doctor["hasFellowship"])
                        doctor["fellowship"] =  data.field_physician_fellowship;
                      else
                        doctor["fellowship"] = null;

                      //hasBoardCerts
                    	if(data.field_physician_board_certs !== null &&
                         data.field_physician_board_certs !== undefined &&
                         data.field_physician_board_certs.length > 0){
                           doctor["hasBoardCerts"] =  true;
                      }
                      else
                        doctor["hasBoardCerts"] =  false;

                      //getBoardCerts
                      if(doctor["hasBoardCerts"])
                        doctor["boardCerts"] =data.field_physician_board_certs;
                      else
                        doctor["boardCerts"] = null;

                      /*
                            $scope.convertToDate = function(dateString) {
                                return new Date(dateString.replace(/-/g,"/"));
                            };
                      */

                      //hasResearchInt
                      if(data.field_research_interests !== undefined &&
                         data.field_research_interests!==null){
                           doctor["hasResearchInt"] = true;
                      }
                      else
                        doctor["hasResearchInt"] = false;

                      //getResearchInt
                      if(doctor["hasResearchInt"])
                        doctor["researchInt"] = data.field_research_interests;
                      else
                      doctor["researchInt"] = null;


                      //hasAnyProfileInformation
                      if(doctor["hasOverview"] || doctor["hasVideoLink"] || doctor["hasSpokenLanguages"] || doctor["hasAreasOfFocus"]){
                        doctor["hasAnyProfileInformation"] = true;
                      }
                      else
                        doctor["hasAnyProfileInformation"] = false;

                      //hasAnyTrainingInformation
                      if(doctor["hasTraining"] || doctor["hasResid"] || doctor["hasFellowship"] || doctor["hasBoardCerts"]){
                        doctor["hasAnyTrainingInformation"] = true;
                      }
                      else
                        doctor["hasAnyTrainingInformation"] = false;

                      //profilePictureUrl
                      if(data.doctor_photo_url !== null && data.doctor_photo_url !== undefined){
                        doctor["profilePictureUrl"] =  data.doctor_photo_url;
                      }
                      else{
                        doctor["profilePictureUrl"] = '/sites/www.dukemedicine.org/themes/dukezen/images/default_physician_photo.png';
                      }

                      doctor["physician_photo"] =  data.field_physician_photo

                      //needsReadMoreAboutMe
                      if(doctor["hasOverview"] && data.field_physician_overview.length > 160)
                        doctor["needsReadMoreAboutMe"] = false;
                      else
                        doctor["needsReadMoreAboutMe"] = true;

                      //needsReadMoreResearchInterests
                      if(doctor["hasResearchInt"] && data.field_research_interests.length > 160)
                        doctor["needsReadMoreResearchInterests"] = false;
                      else
                        doctor["needsReadMoreResearchInterests"] = true;




                        var hasTrialTitle = function(trial){
                          return (trial.title !== null && trial.title !== undefined);
                      };


                      var getTrialTitle = function(trial){
                          return trial.title;
                      };

                      var hasTrialDepartment = function(trial){
                          return trial.field_clin_trial_department.und !== undefined && trial.field_clin_trial_department.und[0].value !== null && trial.field_clin_trial_department.und[0].value !== undefined;
                      };

                      var getTrialDepartment = function(trial){
                          if (hasTrialDepartment(trial)) {
                              return trial.field_clin_trial_department.und[0].value;
                          }
                      };

                      var hasTrialNctNumber = function(trial) {
                          return trial.field_nct_id.und !== undefined && trial.field_nct_id.und[0].safe_value !== null && trial.field_nct_id.und[0].safe_value !== undefined && trial.field_nct_id.und[0].safe_value.indexOf('NCT') > -1;
                      };

                      var getTrialNctNumber = function(trial) {
                          if (hasTrialNctNumber(trial)) {
                              return trial.field_nct_id.und[0].safe_value;
                          }
                      };

                      var hasTrialProNumber = function(trial){
                          return trial.field_clin_trial_eirb.und !== undefined && trial.field_clin_trial_eirb.und[0].value !== null && trial.field_clin_trial_eirb.und[0].value !== undefined;
                      };

                      var getTrialProNumber = function(trial){
                          if (hasTrialProNumber(trial)) {
                              return trial.field_clin_trial_eirb.und[0].value.substring(3);
                          }
                      };

                      var getNonCancerLink = function(trial) {

                          if (hasTrialNctNumber(trial)) {
                              return 'https://clinicaltrials.gov/ct2/show/' + getTrialNctNumber(trial);
                          }
                      };

                      var hasCancerLink = function(trial){
                          return trial.url !== null && trial.url !== undefined && trial.url !== '';
                      };

                      var getCancerLink = function(trial){
                          return trial.url;
                      };

                      var hasCancerTrialTitle = function(trial){
                        return trial.title !== null &&
                        trial.title !== undefined &&
                        trial.title !== '';
                      };

                      var hasCancerTrialRefNumber = function(trial){
                        return trial.field_reference_number.und[0].value !== undefined &&
                        trial.field_reference_number.und[0].value !== null &&
                        trial.field_reference_number.und[0].value !== '';
                      };

                      var getCancerTrialTitle = function(trial){
                        return trial.title;
                      };

                      var getCancerTrialRefNumber = function(trial){
                        return trial.field_reference_number.und[0].value;
                      };

                      var constructTrial = function(trial){
                          if(hasTrialTitle(trial) &&
                             hasTrialDepartment(trial) &&
                             hasTrialProNumber(trial)){
                                 var trialConstruct = {};
                                 trialConstruct.trialName = getTrialTitle(trial);
                                 trialConstruct.trialId = getTrialProNumber(trial);
                                 trialConstruct.department = getTrialDepartment(trial);
                                 if (hasTrialNctNumber(trial)) {
                                     trialConstruct.trialNctNumber = getTrialNctNumber(trial);
                                 }
                                 if (trial.url) {
                                     trialConstruct.link = trial.url;
                                 }
                                 return trialConstruct;
                             }
                      };

                      var constructCancerTrial = function(trial){
                        if(hasCancerTrialTitle(trial) &&
                            hasCancerTrialRefNumber(trial)){
                          var cancerTrialConstruct = {};
                          cancerTrialConstruct.trialName = getCancerTrialTitle(trial);
                          cancerTrialConstruct.trialId = getCancerTrialRefNumber(trial);
                          cancerTrialConstruct.department = 'Oncology';
                          if(hasCancerTrialRefNumber(trial)){
                            cancerTrialConstruct.link = getCancerLink(trial);
                          }
                          if (hasTrialNctNumber(trial)) {
                              cancerTrialConstruct.trialNctNumber = getTrialNctNumber(trial);
                          }
                          return cancerTrialConstruct;
                        }
                      };




                    //clinicalTrials
                     doctor["clinicalTrials"] = [];
                     if(data.clinical_trials !== undefined &&
                       data.clinical_trials !== null &&
                       data.clinical_trials.length > 0){
                         for (var i = 0; i < data.clinical_trials.length; ++i){
                           var trial = constructTrial(data.clinical_trials[i]);
                           if (trial) {
                             doctor["clinicalTrials"].push(trial);
                           }
                          }
                      }

                    //initCancerClinicalTrials
                    doctor["cancerClinicalTrials"] = [];
                    if(data.cancer_clinical_trials !== undefined &&
                      data.cancer_clinical_trials !== null &&
                      data.cancer_clinical_trials.length > 0){
                                for (var i = 0; i < data.cancer_clinical_trials.length; ++i){
                                    var trial = constructCancerTrial(data.cancer_clinical_trials[i]);
                                    if (trial) {
                                      doctor["cancerClinicalTrials"].push(trial);
                                    }
                                }
                            }


                    //hasClinicalTrials
                            if(doctor["clinicalTrials"].length > 0 &&
                               doctor["clinicalTrials"] !== null){
                                 doctor["hasClinicalTrials"] = true;
                            }
                            else
                              doctor["hasClinicalTrials"] = false;

                    //hasCancerClinicalTrials
                            if(doctor["cancerClinicalTrials"].length > 0 &&
                               doctor["cancerClinicalTrials"] !== null){
                                 doctor["hasCancerClinicalTrials"] = true;
                            }
                            else
                              doctor["hasCancerClinicalTrials"] = false;


                    //patientStories
                    if (data.stories !== null && data.stories !== undefined){
                      doctor["patientStories"] = data.stories;
                    }
                    else
                      doctor["patientStories"] = null;

                    //relatedArticles
                    if (data.related_items !== null && data.related_items !== undefined){
                      doctor["relatedArticles"] = data.related_items.blog_post;
                    }
                    else
                      doctor["relatedArticles"] = null;

                    //showPublications
                    doctor["showPublications"] =  !data.field_hide_pubmed_link;

                    //hasPubmedLink
                    if(data.field_pubmed_name !== undefined &&
                       data.field_pubmed_name !== null)
                         doctor["hasPubmedLink"] = true;
                    else
                      doctor["hasPubmedLink"] = false;

                    //getPubmedName
                    if(doctor["hasPubmedLink"]){
                      doctor["pubmedName"]  = encodeURIComponent(data.field_pubmed_name);
                    } else{
                      var pubmedQuery = '';

                      if(doctor["lastName"]){
                        pubmedQuery = pubmedQuery + doctor["lastName"];
                      }
                      if(doctor["firstName"]){
                        pubmedQuery = pubmedQuery + '+' + doctor["firstName"];
                      }
                      if(doctor["middleName"]){
                        pubmedQuery = pubmedQuery + '+' + doctor["middleName"].replace(/\.$/,'').charAt(0);
                      }

                        doctor["pubmedName"]  =  pubmedQuery;
                    }


                    //getPubmedLink
                    var pubmedQuery = 'https://www.ncbi.nlm.nih.gov/pubmed/?term=';

                      doctor["pubmedLink"] =  pubmedQuery +doctor["pubmedName"];


                    // helper functions

                    var isInternalLink = function(link) {
                            return (link.indexOf('http://') === -1) && (link.indexOf('https://') === -1);
                        };




                          defer.resolve(doctor);
                          service.doctorProfile = doctor;

                        } else{
                            defer.reject(data);
                        }
                    }).
                    error(function(data) {
                        defer.reject(data);
                    });
                return defer.promise;






        };



        return {
            fetchDoctorByEpicId: fetchDoctorByEpicId,
            fetchDoctorProfileByPath : fetchDoctorProfileByPath

        };
}]);

'use strict';

angular.module('dukemedicine')
.factory('FetchConfigVariableService', ['$q', '$http', function($q, $http) {

    var getConfigVariable = function(variableName){
      var defer = $q.defer();
      if(variableName !== undefined){
        $http.get('/config-variables?var=' + variableName)
          .success(function(data){
            if(data.error === undefined){
              defer.resolve(data);
            } else{
              defer.reject(data);
            }
          })
          .error(function(error){
              defer.reject(error);
          });
        return defer.promise;
      }
    };

    return {
      getConfigVariable : getConfigVariable
    };

  }
]);

'use strict';


angular.module('dukemedicine')
    .factory('FetchDrupalNodeService', ['$q', '$http', function($q, $http) {
        var node = {};

        // Hashbangs (#!) and everything after get removed by the time
        // the requuest gets to the request handler on the server.
        // So we will split up the url and  send the separate pieces as patams.
        // path is portion before the hashbang.
        // alias is the portion after the hashbang.
        // example: send-card/#!/duke-university-hospital
        //          path=send-card
        //          alias=duke-university-hospital
        var fetchDrupalNode = function(path, alias) {
            var defer = $q.defer();
            var service = this;

            if ((path !== undefined) && alias !== undefined) {
                $http.get('/fetch-node?path=' + path + '&alias=' + alias)
                    .success(function(data) {
                        if(data.error === undefined) {
                            defer.resolve(data);
                            service.node = data;
                        } else{
                            defer.reject(data);
                        }
                    }).
                    error(function(data) {
                        defer.reject(data);
                    });
                return defer.promise;
            }
          /**Needs to be refactored**/
            if ((path !== undefined) && alias === undefined) {
              $http.get('/fetch-node?path=' + path)
                  .success(function(data) {
                    if(data.error === undefined) {
                      defer.resolve(data);
                      service.node = data;
                    } else{
                      defer.reject(data);
                    }
                  }).
                  error(function(data) {
                    defer.reject(data);
                  });
              return defer.promise;
          }
        };

        var fetchDrupalNodeById = function(id) {
            var defer = $q.defer();

            if (id !== undefined && id > 0) {
                $http.get('/fetch-node-by-id?id=' + id)
                    .success(function(data) {
                        if(data.error === undefined) {
                            defer.resolve(data);
                        } else{
                            defer.reject(data);
                        }
                    }).
                    error(function(data) {
                        defer.reject(data);
                    });
                return defer.promise;
            }
        };

        var fetchDrupalNodesByType = function(type) {
            var defer = $q.defer();

            if (type !== undefined && type !== '') {
                $http.get('/fetch-nodes-by-type?type=' + type)
                .success(function(data) {
                    if(data.error === undefined) {
                        defer.resolve(data);
                    } else{
                        defer.reject(data);
                    }
                }).
                    error(function(data) {
                    defer.reject(data);
                });
                return defer.promise;
            }
        };

        var fetchEcentaurusEventNode = function (ecentaurusId){
          var defer = $q.defer();

          if (ecentaurusId !== undefined && ecentaurusId !== '') {
              $http.get('/fetch-ecentaurus-event?ecentaurusId=' + ecentaurusId)
              .success(function(data) {
                  if(data.error === undefined) {
                      defer.resolve(data);
                  } else{
                      defer.reject(data);
                  }
              }).
                  error(function(data) {
                  defer.reject(data);
              });
              return defer.promise;
          }
        };

        return {
            fetchDrupalNode: fetchDrupalNode,
            fetchDrupalNodeById: fetchDrupalNodeById,
            fetchDrupalNodesByType: fetchDrupalNodesByType,
            fetchEcentaurusEventNode: fetchEcentaurusEventNode,
            node: node
        };
}]);

'use strict';


angular.module('dukemedicine')
  .factory('FetchDrupalUserService', ['$q', '$http', function($q, $http){

    var user = {};

    var fetchDrupalUser = function(currentUser){
      user = "Bob";
    };

    return {
      fetchDrupalUser: fetchDrupalUser,
      user: user
    };

  }]);

'use strict';


angular.module('dukemedicine')
.factory('FileService', ['$q','$http', '$location', function($q, $http, $location){

    var getFile = function(fileId, imageStyle){
        var defer = $q.defer();
        var url = $location.protocol() + '://' + $location.host() +
            '/ajax-search/file?id=' + fileId;

        if (imageStyle !== undefined){
            url += '&style=' + imageStyle;
        }

        $http.get(url)
            .success(function(data) {
                defer.resolve(data);
            })
            .error(function(data) {
                defer.reject(data);
            });
        return defer.promise;
    };

    return {
        getFile: getFile
    };
}]);

'use strict';

angular.module('dukemedicine').factory('googleAnalytics',
	function() {

		// Pushes a set page to GA. Url is like: '/name-of-page'
		var setPage = function(url) {
			if(!!window._gaq) {
				window._gaq.push(['_setAccount', 'UA-20097-10']);
				window._gaq.push(['_trackPageview', url]);
			}
		};

		return {
			setPage: setPage
		};
});

'use strict';


angular.module('dukemedicine')
.factory('LocationSearchParams', ['SolrService', function(SolrService){

    var getFilterQueries = function(){
        return [
        ];
    };

    var getQueryFields = function(){
        return [
        ];
    };

    var getBoostQueries = function(){
        return [
        ];
    };

    var getPhraseFields = function(){
        return [
        ];
    };

    var getFacetFields = function(){
        return {
        };
    };

    var getFacetQueries = function(){
        return [
        ];
    };

    var getSpatialParams = function() {
        return {
            'sort': 'geodist() asc',
            'sfield': 'location_coordinates',
            'fl': 'distance:mul(geodist(),0.621371), *',
            'fq': '{!geofilt}',
            'd': '5000'
            // 'sort': 'score desc',
            // 'boost': 'recip(geodist(),1,1000,1000)'
        };
    };

    var getParams = function(facetLimit, start, rows){
        return {
            'start': start,
            'rows': rows,
            'endpoint' : '/locationSearch'
        };
    };

    var setup = function(facetLimit, start, rows){
        SolrService.clearAll();
        SolrService.setFilterQueries(this.getFilterQueries());
        SolrService.setQueryFields(this.getQueryFields());
        SolrService.setBoostQueries(this.getBoostQueries());
        SolrService.setPhraseFields(this.getPhraseFields());
        SolrService.setFacetFields(this.getFacetFields());
        SolrService.setFacetQueries(this.getFacetQueries());
        SolrService.setSpatialParams(this.getSpatialParams());
        SolrService.setParams(this.getParams(facetLimit, start, rows));
    };

    return {
        getFilterQueries: getFilterQueries,
        getQueryFields: getQueryFields,
        getBoostQueries: getBoostQueries,
        getPhraseFields: getPhraseFields,
        getFacetFields: getFacetFields,
        getFacetQueries: getFacetQueries,
        getReturnFields: getFacetQueries,
        getSpatialParams: getSpatialParams,
        getParams: getParams,
        setup: setup
    };
}]);

