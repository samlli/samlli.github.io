            TweenMax.to(element, 0.35, {
                left: '-100%',
                opacity: 0,
                ease: 'Elastic.easOut',
                onComplete: function(){
                    done();
                }
            });
        },
        // Restore width
        removeClass: function(element, className, done) {
            TweenMax.to(element, 0.35, {
                left: '0%',
                opacity: 1,
                ease: 'Elastic.easOut',
                onComplete: function(){
                    done();
                }
            });
        }
    };
});
