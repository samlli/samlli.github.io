angular.module('dukemedicine.system')
.directive('editHead', ['$filter', function ($filter) {
    return{
        restrict: 'A',
        scope: {
            node: '=',
            image: '='
        },
        link: function(scope, element, attrs) {


            var updateHead = function(scope, attrs) {
                var node = scope.node;
                var imageUrl = scope.image;
                var el = null;
                var head = angular.element('head');
                var seoMetaDescription = "";

                // set meta description
                if(scope.$parent.seoMetaDescription !== undefined && scope.$parent.seoMetaDescription !== null)
                  seoMetaDescription = scope.$parent.seoMetaDescription;
                else if (attrs.fieldMetaDesc !== null && attrs.fieldMetaDesc !== undefined && attrs.fieldMetaDesc !== '')
                  seoMetaDescription =   attrs.fieldMetaDesc();

                el = angular.element('meta[name="description"]');
                if (el !== undefined) {
                        el.attr('content', seoMetaDescription);
                    }

                    // Open Graph tag used by facebook, google+
                    head.append('<meta property="og:description" itemprop="description" content="' + seoMetaDescription + '" />');


                // set meta abstract
                if (attrs.fieldMetaAbstract !== null && attrs.fieldMetaAbstract !== undefined && attrs.fieldMetaAbstract !== '') {
                    el = angular.element('meta[name="abstract"]');
                    if (el !== undefined) {
                        el.attr('content', $filter('htmlToPlainText')(attrs.fieldMetaAbstract));
                    }
                }

                // set title
                var title = '';
                if (scope.$parent.title !== null && scope.$parent.title !== undefined )
                {
                  title = scope.$parent.title() + ' | ' + 'Duke Medicine';
                }
                else if (node !== undefined && node.title !== null && node.title !== undefined) {
                    title = node.title + ' | ' + 'Duke Medicine';
                } else if (attrs.fieldTitle !== null && attrs.fieldTitle !== undefined) {
                    title = scope.$parent[attrs.fieldTitle]() + ' | ' + 'Duke Medicine';
                } else {
                    title = 'Duke Medicine';
                }


                el = angular.element('title');
                if (el !== undefined) {
                    el.text(title);
                }

                // og:title Open Graph tag used by facebook, google+
                head.append('<meta property="og:title" itemprop="name" content="' + title + '" />');

                // og:url Open Graph tag used by facebook, google+
                if (node !== undefined && node.url !== null && node.url !== undefined) {
                    if (node.url.indexOf('#!') !== -1) {
                        node.url = node.url.replace('/#!', '');
                    }

                    head.append('<meta property="og:url" content="' + node.url + '" />');
                    el = angular.element('link[rel="canonical"]');
                    if (el !== undefined) {
                        el.attr('href', node.url);
                    }
                }

                // og:site_name Open Graph tag used by facebook, google+
                head.append('<meta property="og:site_name" content="Duke Medicine" />');

                if(imageUrl !== undefined && imageUrl !== null){
                    // og:image Open Graph tag used by facebook, google+
                    head.append('<meta property="og:image" itemprop="image" content="' + imageUrl + '" />');
                }
            };

            angular.element(document).ready(updateHead(scope, attrs));

        }
    };

}]);

'use strict';

angular.module('dukemedicine')
    .directive('ngFacetFilter', function() {
        return {
            restrict: 'A',
            transclude: true,
            scope: {
                combineType: '@',
                facets: '=',
                facetLimit: '@',
                searchField: '@'
            },
            template: '<div data-ng-transclude class="facet-heading" data-ng-if="hasFacets()" data-ng-click="toggleCollapse()" data-ng-class="{collapsed: isCollapsed}"></div>' +
                    '<ul class="section-collapsed" data-ng-hide="isCollapsed">'+
                        '<li data-ng-class="{ \'selected-facet\': isActive($index)}" data-ng-repeat="facet in facets | limitTo: facetLimit" data-ng-click="modifyFacet($index)">' +
                            '<span class="icon-stack" data-ng-if="combineType">' +
                                '<i class="icon-check-empty icon-stack-base"></i>' +
                                '<i data-ng-if="isActive($index)" class="icon-ok"></i>' +
                            '</span>' +
                            '<span data-ng-if="!combineType" class="single-icon"><i class="icon-ok" data-ng-if="isActive($index)"></i></span>'+
                            '<span id="facet-name">{{facet[0]}}</span> <span class="count">{{facet[1]}}</span>' +
                        '</li>' +
                        '<a data-ng-show="needsShow()" data-ng-click="toggleFacetLimit()" class="see-more">{{showText}}<i data-ng-class="{\'icon-chevron-sign-down\': showMore, \'icon-chevron-sign-up\': !showMore}"></i></a>' +
                    '</ul>',
            controller: function($scope, SolrService, $location) {
                $scope.location = $location;
                $scope.solr = SolrService;
                $scope.oldLimit = -1;
                $scope.isCollapsed = true;
                $scope.showMore = true;
                $scope.showText = 'See more';

                $scope.toggleCollapse = function() {
                    $scope.isCollapsed = !$scope.isCollapsed;
                };

                $scope.hasFacets = function(){
                    return $scope.facets.length > 0 ? 1 : 0;
                };

                $scope.modifyFacet = function(index) {
                    var facetString = $scope.searchField + ':',
                        facetValue = $scope.facets[index][0];
                    // If the facet already exists in the URL
                    if($scope.location.$$search.fq !== undefined && $scope.location.$$search.fq !== ''){
                        var facetregexp, doublefacetregexp, lastfacetregexp, firstfacetregexp, holder;
                        // If there is only one FQ active
                        if(typeof $scope.location.$$search.fq === 'string'){
                            if($scope.location.$$search.fq.indexOf($scope.searchField) === -1){
                                $scope.location.search('fq', [$scope.location.$$search.fq, facetString + '"' + facetValue + '"']).search('page' , 0);
                            } else{
                                // If the facet is already active, remove
                                if($scope.location.$$search.fq.indexOf(facetValue) !== -1){
                                    if($scope.location.$$search.fq.indexOf($scope.combineType) !== -1){
                                        facetregexp = new RegExp('"' + facetValue + '"', 'g');
                                        doublefacetregexp = new RegExp($scope.combineType + '  ' + $scope.combineType, 'g');
                                        lastfacetregexp = new RegExp(' ' + $scope.combineType + ' \\)', 'g');
                                        firstfacetregexp = new RegExp('\\( ' + $scope.combineType + ' ', 'g');
                                        holder = $scope.location.$$search.fq;

                                        holder = holder.replace(facetregexp, '').replace(doublefacetregexp, $scope.combineType).replace(lastfacetregexp, ')').replace(firstfacetregexp, '(');

                                        $scope.location.search('fq', holder).search('page' , 0);
                                    } else{
                                        try {
                                            delete $scope.location.$$search.fq;
                                        }
                                        catch(e) {
                                            $scope.location.$$search.fq = '';
                                        }
                                        $scope.location.search($scope.location.$$search).search('page' , 0);
                                    }
                                } else{
                                    // If there are multiple facets active on the field, add to the end
                                    if($scope.location.$$search.fq.indexOf($scope.combineType) !== -1){
                                        $scope.location.search('fq', $scope.location.$$search.fq.replace(/\)/g, '') + ' ' + $scope.combineType + ' "'+ facetValue +'")').search('page' , 0);
                                    } else{
                                        // Strip the quotes, remove the searchfield and get the last facet
                                        $scope.location.search('fq', facetString + '(' + $scope.location.$$search.fq.split(':')[1] + ' ' + $scope.combineType + ' "' + facetValue + '")').search('page' , 0);
                                    }
                                }
                            }
                        }
                        // If there are multiple FQs, loop through, find the correct index, then check for exitance
                        else if(typeof $scope.location.$$search.fq === 'object'){
                            if($scope.location.$$search.fq.length === 0){
								$scope.location.$$search.fq.push(facetString + '"' + facetValue + '"');
								$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
							} else{
								for (var i = $scope.location.$$search.fq.length - 1; i >= 0; i--) {
									if($scope.location.$$search.fq[i].indexOf($scope.searchField) !== -1){
										// If the facet is already active, remove
										if($scope.location.$$search.fq[i].indexOf(facetValue) !== -1){
											if($scope.location.$$search.fq[i].indexOf($scope.combineType) !== -1){
												facetregexp = new RegExp('"' + facetValue + '"', 'g');
												doublefacetregexp = new RegExp($scope.combineType + '  ' + $scope.combineType, 'g');
												lastfacetregexp = new RegExp(' ' + $scope.combineType + ' \\)', 'g');
												firstfacetregexp = new RegExp('\\( ' + $scope.combineType + ' ', 'g');
												holder = $scope.location.$$search.fq[i];

												$scope.location.$$search.fq[i] = holder.replace(facetregexp, '').replace(doublefacetregexp, $scope.combineType).replace(lastfacetregexp, ')').replace(firstfacetregexp, '(');

												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											} else{
												$scope.location.$$search.fq.splice(i, 1);
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											}
										} else{
											// If there are multiple facets active on the field, add to the end
											if($scope.location.$$search.fq[i].indexOf($scope.combineType) !== -1){
												$scope.location.$$search.fq[i] = $scope.location.$$search.fq[i].replace(/\)/g, '') + ' ' + $scope.combineType + ' "'+ facetValue +'")';
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											} else{
												$scope.location.$$search.fq[i] = facetString + '(' + $scope.location.$$search.fq[i].split(':')[1] + ' ' + $scope.combineType + ' "' + facetValue + '")';
												// Strip the quotes, remove the searchfield and get the last facet
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											}
										}
									} else if(i === 0){
										$scope.location.$$search.fq.push(facetString + '"' + facetValue + '"');
										$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
										break;
									}
								}
							}
                        }
                    }
                    // Base case adding to the activeFacets var
                    else{
                        $scope.location.search('fq', facetString + '"' + facetValue + '"').search('page' , 0);
                    }
                };

                /* Toggles the show more or show less facets */
                $scope.toggleFacetLimit = function() {
                    if($scope.oldLimit < 0) {
                        $scope.oldLimit = $scope.facetLimit;
                        $scope.facetLimit = 9999;
                        $scope.showText = 'Show less';
                        $scope.showMore = false;
                    } else{
                        $scope.facetLimit = $scope.oldLimit;
                        $scope.oldLimit = -1;
                        $scope.showText = 'Show more';
                        $scope.showMore = true;
                    }
                };

                /* Determines if a facet is in the URL or not */
                $scope.isActive = function(index) {
                    if($scope.location.$$search.fq !== undefined){
                        // If there is only one FQ in the URL
                        if(typeof $scope.location.$$search.fq === 'string' && $scope.location.$$search.fq.indexOf($scope.facets[index][0]) > -1){
                            $scope.isCollapsed = false;
                            return true;
                        }
                        // If there are multiple FQs, loop through, find the correct index, then check for exitance
                        else if(typeof $scope.location.$$search.fq === 'object') {
                            for (var i = $scope.location.$$search.fq.length - 1; i >= 0; i--) {
                                if(($scope.location.$$search.fq[i]).indexOf($scope.searchField) !== -1){
                                    if(($scope.location.$$search.fq[i]).indexOf($scope.facets[index][0]) !== -1){
                                        return true;
                                    } else{
                                        return false;
                                    }
                                }
                            }
                        }
                    }
                    return false;
                };

                $scope.hasFacetField = function(){
                    return $scope.location.$$search.fq.hasOwnProperty($scope.searchField);
                };

                $scope.needsShow = function() {
                    if($scope.facets !== undefined){
                        if($scope.facets.length > $scope.facetLimit || $scope.facetLimit === 9999){
                            return true;
                        }
                    }
                    return false;
                };
            },
            link: function($scope, element, attrs) {
                $scope.combineType = attrs.combineType !== undefined ? attrs.combineType : '';
            }
        };
    });

'use strict';

angular.module('dukemedicine')
    .directive('collapse', ['$animate', function($animate){
        return {
            link: function(scope, element, attrs) {

                scope.$watch(attrs.grow, function (shouldCollapse, oldValue) {
                    if(shouldCollapse !== oldValue){
                        if (shouldCollapse) {
                            $animate.addClass(element, 'grow');
                        } else {
                            $animate.addClass(element, 'shrink');
                        }
                    }
                });
            }
        };
    }])
    .animation('.grow', function() {
        return{
            addClass: function(element, className, done) {
                var remove = function(){
                    element.removeClass('expand');
                    element.css('height', 'auto');
                    done();
                };

                if(element.attr('oldHeight') !== undefined){
                    TweenMax.to(element, 0.25, {
                        height: element.attr('oldHeight'),
                        borderColor: '#CCC',
                        ease: 'Elastic.easOut',
                        onComplete: remove
                    });
                } else{
                    TweenMax.to(element, 0.25, {
                        minHeight: '1000px',
                        borderColor: '#CCC',
                        ease: 'Elastic.easOut',
                        onComplete: remove
                    });
                }

            }
        };
    })
    .animation('.shrink', function() {
        return{
            beforeAddClass: function(element, className, done) {
                element.attr('oldHeight', element.css('height'));
                done();
            },
            addClass: function(element, className, done) {
                var remove = function(){
                    element.removeClass('collapse');
                    done();
                };

                TweenMax.to(element, 0.25, {
                    height: '0px',
                    borderColor: '#FFF',
                    ease: 'Elastic.easOut',
                    onComplete: remove
                });
            }
        };
    });

'use strict';

angular.module('dukemedicine').directive('ngInstantQuery', function () {
    return {
        restrict: 'A',
        template: '<form data-ng-submit="submitQuery()" class="input-prepend">' +
        '<input class="ajax-search-box" type="text" placeholder="{{placeholderText}}" data-ng-model="query">' +
        '<button type="submit" class="add-on"><i class="icon-search"></i></button>' +
        '</div>',
        scope: {
            searchType: '='
        },
        controller: function($scope, $location, $rootScope, $browser, Debounce, QueryService) {
            $scope.location = $location;
            $scope.query = QueryService.getQuery();
            $scope.queryService = QueryService;
			$scope.debounce = Debounce;

            $rootScope.$on('$locationChangeSuccess', function() {
                $scope.query = $scope.queryService.getQuery();
            });


            $scope.submitQuery = function() {

                if($scope.location.$$search.page !== undefined && $scope.location.$$search.page > 0){
                    $scope.location.search({s: $scope.query, page: 0});
                } else{
                    $scope.location.search({s: $scope.query});
                }

            };

            $scope.$watch('query', function(newValue, oldValue){
                $scope.query = newValue;
				if(oldValue !== undefined && newValue !== oldValue){
					$scope.makeQueryDebounce(newValue);
				}
            });

			$scope.makeQueryDebounce = $scope.debounce($scope.submitQuery, 425, false);

            $scope.$on('setPlaceholderText', function(sender, text){
                $scope.placeholderText = text;
            });
        },
        link: function(scope, element, attrs) {
            if (attrs.url !== undefined) {
                scope.url = attrs.url;
            }

            if (attrs.ignorePath !== undefined) {
                scope.ignorePath = attrs.ignorePath;
            }

            if (attrs.placeholderText !== undefined) {
                scope.placeholderText = attrs.placeholderText;
            }

            element.bind('submit', function() {
                element.find('input').blur();
            });
        }
    };
});

'use strict';

angular.module('dukemedicine')
    .directive('ngNestedFacetFilter', ['NestedFacets', function(NestedFacets) {
        return {
            restrict: 'A',
            transclude: true,
            scope: {
                combineType: '@',
                facets: '=',
                facetLimit: '@',
                searchField: '@'
            },
            template:   '<div data-ng-transclude class="facet-heading" data-ng-if="hasFacets()" data-ng-click="toggleCollapse()" data-ng-class="{collapsed: isCollapsed}">' +
                            '<!-- category heading will go here due to transclusion -->' +
                        '</div>' +
                        '<ul class="section-collapsed nested-facet-filter" data-ng-hide="isCollapsed" data-ng-class="{collapsed: isCollapsed}">' +
                            '<li class="facet" data-ng-repeat="facet in facets | orderBy: \'name\' | limitTo: limit" data-ng-class="{active: facet.active, last: $last}">' +
                                '<span class="collapse-button" data-ng-class="{\'icon-plus-sign\': facet.collapsed, \'icon-minus-sign\': !facet.collapsed, \'hidden\': facet.children.length == 0}" data-ng-click="facet.collapsed = !facet.collapsed"></span>' +
                                '<span class="active-label" data-ng-click="toggleTopLevelFacet(facet)">' +
                                    '<span class="icon-stack">' +
                                        '<i class="icon-check-empty icon-stack-base"></i>' +
                                        '<i data-ng-if="isActive(facet)" class="icon-ok"></i>' +
                                    '</span>' +
                                    '<span class="facet-name">{{facet.name}}</span>' +
                                '</span>' +
                                '<ul class="subfacet-container section-collapsed" data-ng-hide="facet.collapsed">' +
                                    '<li class="subfacet" data-ng-repeat="subfacet in facet.children | orderBy: \'name\' | limitTo: subfacetLimit(facet)" data-ng-show="facet.children.length > 0" data-ng-class="{last: $last}">' +
                                        '<span class="active-label" data-ng-click="modifyFacet(subfacet)">' +
                                            '<span class="icon-stack">' +
                                                '<i class="icon-check-empty icon-stack-base"></i>' +
                                                '<i data-ng-if="isActive(subfacet)" class="icon-ok"></i>' +
                                            '</span>' +
                                            '<span class="facet-name">{{subfacet.name}}</span>' +
                                        '</span>' +
                                    '</li>' +
                                    '<li class="show-more" data-ng-show="subfacetNeedsShow(facet)">' +
                                        '<a data-ng-click="toggleSubfacetLimit(facet)" class="see-more"><i data-ng-class="{\'icon-chevron-sign-down\': facet.limit == facetLimit, \'icon-chevron-sign-up\': facet.limit != facetLimit}"></i> {{subfacetShowText(facet)}}</a>' +
                                    '</li>' +
                                '</ul>' +
                            '</li>' +
                            '<li class="show-more" data-ng-show="facetNeedsShow()">' +
                                '<a data-ng-click="toggleFacetLimit()" class="see-more"><i data-ng-class="{\'icon-chevron-sign-down\': limit == facetLimit, \'icon-chevron-sign-up\': limit != facetLimit}"></i> {{facetShowText()}}</a>' +
                            '</li>' +
                        '</ul>',
            controller: function($scope, $location) {
                $scope.location = $location;
                $scope.isCollapsed = true;

                $scope.toggleCollapse = function() {
                    $scope.isCollapsed = !$scope.isCollapsed;
                }

                $scope.hasFacets = function(){
                    return ($scope.facets === undefined || $scope.facets.length > 0);
                };

                $scope.toggleTopLevelFacet = function(facet) {
                    // 1. if the top is active and toggled, then
                    //      * all children off
                    //      * top off
                    // 2. if the top is off and toggled, then
                    //      * all children on
                    //      * top on
                    var isInitiallyActive = $scope.isActive(facet);
                    var endResultShouldBeOn = !isInitiallyActive;

                    for (var i = 0; i < facet.children.length; i++) {
                        var subfacet = facet.children[i]
                        // if it should be on
                        if (endResultShouldBeOn) {
                            // but it is off
                            if (!$scope.isActive(subfacet)) {
                                // then turn it on
                                $scope.modifyFacet(subfacet);
                                subfacet.active = true;
                            }
                        // if it should be off
                        } else {
                            // but it is on
                            if ($scope.isActive(subfacet)) {
                                // turn it off
                                $scope.modifyFacet(subfacet);
                                subfacet.active = false;
                            }
                        }
                    }

                    // if it should be on
                    if (endResultShouldBeOn) {
                        // but it is off
                        if (!$scope.isActive(facet)) {
                            // then turn it on
                            $scope.modifyFacet(facet);
                            facet.active = true;
                        }
                    // if it should be off
                    } else {
                        // but it is on
                        if ($scope.isActive(facet)) {
                            // turn it off
                            $scope.modifyFacet(facet);
                            facet.active = false;
                        }
                    }
                }

                $scope.modifyFacet = function(facet) {
                    var facetString = $scope.searchField + ':',
                        facetValue = facet.name;
                    facet.active = !facet.active; // toggle the active state of the facet
                    // If the facet already exists in the URL
                    if($scope.location.$$search.fq !== undefined && $scope.location.$$search.fq !== ''){
                        var facetregexp, doublefacetregexp, lastfacetregexp, firstfacetregexp, holder;
                        // If there is only one FQ active
                        if(typeof $scope.location.$$search.fq === 'string'){
                            if($scope.location.$$search.fq.indexOf($scope.searchField) === -1){
                                $scope.location.search('fq', [$scope.location.$$search.fq, facetString + '"' + facetValue + '"']).search('page' , 0);
                            } else{
                                // If the facet is already active, remove
                                if($scope.location.$$search.fq.indexOf(facetValue) !== -1){
                                    if($scope.location.$$search.fq.indexOf($scope.combineType) !== -1){
                                        facetregexp = new RegExp('"' + facetValue + '"', 'g');
                                        doublefacetregexp = new RegExp($scope.combineType + '  ' + $scope.combineType, 'g');
                                        lastfacetregexp = new RegExp(' ' + $scope.combineType + ' \\)', 'g');
                                        firstfacetregexp = new RegExp('\\( ' + $scope.combineType + ' ', 'g');
                                        holder = $scope.location.$$search.fq;

                                        holder = holder.replace(facetregexp, '').replace(doublefacetregexp, $scope.combineType).replace(lastfacetregexp, ')').replace(firstfacetregexp, '(');

                                        $scope.location.search('fq', holder).search('page' , 0);
                                    } else{
                                        try {
                                            delete $scope.location.$$search.fq;
                                        }
                                        catch(e) {
                                            $scope.location.$$search.fq = '';
                                        }
                                        $scope.location.search($scope.location.$$search).search('page' , 0);
                                    }
                                } else{
                                    // If there are multiple facets active on the field, add to the end
                                    if($scope.location.$$search.fq.indexOf($scope.combineType) !== -1){
                                        $scope.location.search('fq', $scope.location.$$search.fq.replace(/\)/g, '') + ' ' + $scope.combineType + ' "'+ facetValue +'")').search('page' , 0);
                                    } else{
                                        // Strip the quotes, remove the searchfield and get the last facet
                                        $scope.location.search('fq', facetString + '(' + $scope.location.$$search.fq.split(':')[1] + ' ' + $scope.combineType + ' "' + facetValue + '")').search('page' , 0);
                                    }
                                }
                            }
                        }
                        // If there are multiple FQs, loop through, find the correct index, then check for exitance
                        else if(typeof $scope.location.$$search.fq === 'object'){
                            if($scope.location.$$search.fq.length === 0){
								$scope.location.$$search.fq.push(facetString + '"' + facetValue + '"');
								$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
							} else{
								for (var i = $scope.location.$$search.fq.length - 1; i >= 0; i--) {
									if($scope.location.$$search.fq[i].indexOf($scope.searchField) !== -1){
										// If the facet is already active, remove
										if($scope.location.$$search.fq[i].indexOf(facetValue) !== -1){
											if($scope.location.$$search.fq[i].indexOf($scope.combineType) !== -1){
												facetregexp = new RegExp('"' + facetValue + '"', 'g');
												doublefacetregexp = new RegExp($scope.combineType + '  ' + $scope.combineType, 'g');
												lastfacetregexp = new RegExp(' ' + $scope.combineType + ' \\)', 'g');
												firstfacetregexp = new RegExp('\\( ' + $scope.combineType + ' ', 'g');
												holder = $scope.location.$$search.fq[i];

												$scope.location.$$search.fq[i] = holder.replace(facetregexp, '').replace(doublefacetregexp, $scope.combineType).replace(lastfacetregexp, ')').replace(firstfacetregexp, '(');

												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											} else{
												$scope.location.$$search.fq.splice(i, 1);
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											}
										} else{
											// If there are multiple facets active on the field, add to the end
											if($scope.location.$$search.fq[i].indexOf($scope.combineType) !== -1){
												$scope.location.$$search.fq[i] = $scope.location.$$search.fq[i].replace(/\)/g, '') + ' ' + $scope.combineType + ' "'+ facetValue +'")';
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											} else{
												$scope.location.$$search.fq[i] = facetString + '(' + $scope.location.$$search.fq[i].split(':')[1] + ' ' + $scope.combineType + ' "' + facetValue + '")';
												// Strip the quotes, remove the searchfield and get the last facet
												$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
												break;
											}
										}
									} else if(i === 0){
										$scope.location.$$search.fq.push(facetString + '"' + facetValue + '"');
										$scope.location.search('fq', $scope.location.$$search.fq).search('page' , 0);
										break;
									}
								}
							}
                        }
                    }
                    // Base case adding to the activeFacets var
                    else{
                        $scope.location.search('fq', facetString + '"' + facetValue + '"').search('page' , 0);
                    }
                };

                $scope.showLessText = 'Show less';
                $scope.showMoreText = 'Show more';

                $scope.limit = $scope.facetLimit;
                $scope.showText = $scope.showMoreText;

                $scope.toggleFacetLimit = function() {
                    if ($scope.limit == $scope.facetLimit) {
                        $scope.limit = $scope.maxLimit;
                        $scope.showText = $scope.showLessText;
                    } else {
                        $scope.limit = $scope.facetLimit;
                        $scope.showText = $scope.showMoreText;
                    }
                }

                /* Toggles the show more or show less for subfacets */
                $scope.toggleSubfacetLimit = function(facet) {
                    // if($scope.oldLimit < 0) {
                    //     $scope.oldLimit = $scope.facetLimit;
                    //     $scope.facetLimit = 9999;
                    //     $scope.showText = 'Show less';
                    //     $scope.showMore = false;
                    // } else{
                    //     $scope.facetLimit = $scope.oldLimit;
                    //     $scope.oldLimit = -1;
                    //     $scope.showText = 'Show more';
                    //     $scope.showMore = true;
                    // }

                    // facet.showText
                    // facet.limit
                    if (facet.limit === undefined || facet.limit === $scope.facetLimit) {
                        facet.limit = $scope.maxLimit;
                        facet.showText = $scope.showLessText;
                    } else if (facet.limit === $scope.maxLimit) {
                        facet.limit = $scope.facetLimit;
                        facet.showText = $scope.showMoreText;
                    }
                };

                $scope.maxLimit = Number.MAX_SAFE_INTEGER;

                $scope.facetLimit = function() {
                    return $scope.limit;
                }

                // returns the facet number limit, or the specific limit if defined
                $scope.subfacetLimit = function(facet) {
                    // if the facet doesn't have a limit defined, initialize it
                    if (facet.limit === undefined) {
                        facet.limit = $scope.facetLimit;
                        return facet.limit;
                    // if the facet is at the max, set to the max limit
                    // https://docs.angularjs.org/api/ng/filter/limitTo
                    } else if (facet.limit == $scope.maxLimit) {
                        return Number.MAX_SAFE_INTEGER;
                    // if the facet has a normal limit, return that
                    } else {
                        return facet.limit;
                    }
                };

                $scope.facetShowText = function() {
                    return $scope.showText;
                };

                $scope.subfacetShowText = function(facet) {
                    if (facet.showText !== undefined) {
                        return facet.showText;
                    } else {
                        return $scope.showMoreText;
                    }
                }

                /* Determines if a facet is in the URL or not */
                $scope.isActive = function(facet) {
                    // return facet.active;
                    if($scope.location.$$search.fq !== undefined){
                        // If there is only one FQ in the URL
                        if(typeof $scope.location.$$search.fq === 'string' && $scope.location.$$search.fq.indexOf(facet.name) > -1){
                            $scope.isCollapsed = false;
                            return true;
                        }
                        // If there are multiple FQs, loop through, find the correct index, then check for exitance
                        else if(typeof $scope.location.$$search.fq === 'object') {
                            for (var i = $scope.location.$$search.fq.length - 1; i >= 0; i--) {
                                if(($scope.location.$$search.fq[i]).indexOf($scope.searchField) !== -1){
                                    if(($scope.location.$$search.fq[i]).indexOf(facet.name) !== -1){
                                        return true;
                                    } else{
                                        return false;
                                    }
                                }
                            }
                        }
                    }
                    return false;
                };

                $scope.hasFacetField = function(){
                    return $scope.location.$$search.fq.hasOwnProperty($scope.searchField);
                };

                $scope.facetNeedsShow = function() {
                    return $scope.facets.length > $scope.facetLimit;
                };

                $scope.subfacetNeedsShow = function(facet) {
                    // if($scope.facets !== undefined){
                    //     if($scope.facets.length > $scope.facetLimit || $scope.facetLimit === 9999){
                    //         return true;
                    //     }
                    // }
                    // return false;
                    if (facet.children == undefined || facet.children.length == 0) {
                        return false;
                    } else if (facet.children.length > $scope.facetLimit) {
                        return true;
                    } else {
                        return false;
                    }
                };
            },
            link: function($scope, element, attrs) {
                $scope.combineType = attrs.combineType !== undefined ? attrs.combineType : '';
            }
        };
    }]);

'use strict';

angular.module('dukemedicine')
    .directive('myOverviewGroupMedia', function () {
        return {
            restrict: 'A',
            scope: {
                item: '='
            },
            template: '<img src="{{imageUrl}}" alt="" typeof="foaf:Imaage">',
            controller: function($scope, FileService) {
                if ($scope.$parent.overviewGroupMedia() !== undefined){
                    FileService.getFile($scope.$parent.overviewGroupMedia().fid)
                        .then(function(data) {
                            if(data.error === undefined) {
                                $scope.imageUrl = data.image_url ? data.image_url : '';
                            } else {
                                console.log(data.error);
                            }
                        }, function(errorData) {
                            console.log(errorData.error);
                        });

                }
            }
        };
    });

'use strict';

angular.module('dukemedicine')
    .directive('myPatientResources', function () {
        return {
            restrict: 'A',
            scope: {
                resource: '='
            },
            template:
                '<i class="icon-li icon-angle-right"></i>' +
                '<a href="{{resource.url}}" target="{{resource.target}}">{{resource.title}}</a>',

            link: function(scope, element) {
                // Run jquery plugin columnizer AFTER all elements are added to the DOM.
                if (scope.$parent.$last){
                    // Bind click event to collapsed items in mobile view.
                    element.parent().parent().find('.collapseable').on('click', function(){
                        element.parent().parent().toggleClass('hidden');
                    });
                }

            }
        };
    });

'use strict';

angular.module('dukemedicine')
    .directive('myPatientStoryFlexslider', function () {
        return {
            restrict: 'A',
            scope: {
                story: '='
            },
            template:
                '<div class="patient-image">' +
                    '<img src="{{story.image.url}}" alt="{{story.image.alt}}" title="{{story.image.title}}">' +
                '</div>' +
                '<span class="patient-story-info">' +
                    '<span class="section-title">PATIENT STORY</span>' + 
                    '<a href="{{story.url}}"><div class="story-title">{{story.title}}</div></a>' +
                    '<div class="flex-caption" data-ng-bind-html="story.quote"></div>' +
                '</span>',
            link: function(scope, element) {
                // Run jquery plugin columnizer AFTER all elements are added to the DOM.
                if (scope.$parent.$last){
                    element.parent().parent().addClass('flexslider');
                    element.parent().parent().flexslider({
                        animation: 'fade',
                        slideshow: false,
                        pauseOnHover: true,
                        start: function(slider) {
                            slider.removeClass('loading');
                        }
                    });
                }
            }
        };
    });

'use strict';

angular.module('dukemedicine').directive('queryPrediction', [function(){
	// Runs during compile
	return {
		// name: '',
		// priority: 1,
		// terminal: true,
		scope: {
			query: '=',
			completionSource: '=',
			suggestionSource: '=',
			alternativeSource: '=',
			rateLimit: '=',
			resultLimit: '=',
			emptyOnEmptyQuery: '=',
			placeOnParentScope: '=',
			fragments: '=',
			caseSensitive: '=',
		}, // {} = isolate, true = child, false/undefined = no change
		controller: function($scope, $element, $attrs, $transclude, Debounce) {
			// default $scope.options values if they aren't set
			var defaults = {
				rateLimit: 500,
				resultLimit: 3,
				emptyOnEmptyQuery: true,
				placeOnParentScope: false,
				fragments: false,
				caseSensitive: false
			};

			$scope.options = {};

			// copy the defaults to $scope.options, but give precendence to existing attributes
			for(property in defaults){
				if ($scope[property] == undefined) {
					$scope.options[property] = defaults[property];
				} else {
					$scope.options[property] = $scope[property];
				}
			}
			// $scope.options
			// angular.extend($scope.options, defaults);

			// log an error if there aren't any completion sources specified
			if ($scope.completionSource === undefined && $scope.suggestionSource === undefined && $scope.alternativeSource === undefined) {

			}

			var predict = function(newQuery) {

				// make sure we start with a clean set of predictions
				$scope.emptyPredictions();

				// is our query case-sensitive?
				if ($scope.options.caseSensitive == false) {
					newQuery = newQuery.toLowerCase();
				}

				var resultHandler = function(results) {


					// if we just cleared the query, then don't put the result on the scope
					if (!$scope.preventNewPredictionsFromBeingPlacedOnScope) {
						pushPredictionsOnScope(results);
					}
				};

				var errorHandler = function(error) {
					$scope.predictions = [];
				}

				// call the various sources for results if they're defined
				if ($scope.completionSource !== undefined) {
					$scope.completionSource(newQuery).then(resultHandler, errorHandler);
				}
				if ($scope.suggestionSource !== undefined) {
					$scope.suggestionSource(newQuery).then(resultHandler, errorHandler);
				}
				if ($scope.alternativeSource !== undefined) {
					$scope.alternativeSource(newQuery).then(resultHandler, errorHandler);
				}

			};

			// update the scope with the new predictions in a way that respects the fragments and result limit option
			// expects an array of predictions as an arugment
			// also respects the result limit option
			var pushPredictionsOnScope = function(predictions) {

				// this operation performs differently dending on what structure of $scope.predictions is, which is determined by the fragments option


				// complex predictions object
				if ($scope.options.fragments) {

					// update the metadata
					if ($scope.options.caseSensitive) {
						$scope.predictions.query = $scope.query;
					} else {
						$scope.predictions.query = $scope.query.toLowerCase();
					}
					$scope.predictions.resultsFound += predictions.length;
					$scope.predictions.resultLimit = $scope.options.resultLimit;

					var remainingAvailablePredictions = $scope.predictions.resultLimit - $scope.predictions.predictions.length;

					// only continue if we aren't already at the result limit
					if (remainingAvailablePredictions > 0) {

						// add just enough predictions to the result list to fill it up (as long as we have enough)
						for (var i = 0; i < remainingAvailablePredictions && i < predictions.length; i++) {
							// break into fragments (really an array of fragment objects) and add to the predictions
							var fragment = breakIntoFragment(predictions[i], $scope.predictions.query);
							$scope.predictions.predictions.push(fragment);
						}

					}

				// simple predictions array
				} else {

					// holds our master list of predicted queries
					// start with the existing predictions, if there are any
					var newPredictions = $scope.predictions;

					// add the results to the prediction list
					for (var i = 0; i < predictions.length; i++) {
						newPredictions.push(predictions[i]);

						// if we've hit the limit, stop and return
						if (newPredictions.length >= $scope.options.resultLimit) {
							newPredictions.length = $scope.options.resultLimit; // need this in case we just added 1 prediction too many
							$scope.predictions = newPredictions;
							return;
						}
					}

					// we didn't hit the limit, but return what we have
					$scope.predictions = predictions;
				}
			};

			var breakIntoFragment = function(prediction, query) {
				// what we return is a prediction represented as an array of objects, with each object reprenting a fragment (string part) of the prediction
				// match up to three groups: before query, query, and after query
				var fragments = [];
				var queryStartingPosition = prediction.indexOf(query);
				var queryLength = query.length;

				// fragment
				fragments[0] = prediction.substr(0, queryStartingPosition);
				fragments[1] = query;
				fragments[2] = prediction.substr(queryStartingPosition + queryLength, prediction.length);

				// remove empty fragments
				fragments = fragments.filter(function(n){return n != ""});

				// objectify the fragments
				fragments = fragments.map(function(f) {
					return {
						fragment: f,
						matching: f == query
					};
				});

				// fragments is an array of up to three elements
				// one of the elements could (should!) be the query exactly


				return fragments;
			};

			// set-up rate limiting on the prediction function

			var debouncedPredict = Debounce(predict, $scope.options.rateLimit, false);

			// calls the debounced predict(...) function when the query changes
			var watchQueryToUpdatePredictions = function(newQuery, oldQuery) {


				// check if empty query
				if ($scope.options.emptyOnEmptyQuery && newQuery == "") {
					// empty query, and we are told to empty the prediction list when that's the case

					// the preventNewPredictionsFromBeingPlacedOnScope variable is used to prevent the any debounced query that hasn't come back yet from adding results to the predictions
					$scope.preventNewPredictionsFromBeingPlacedOnScope = true;
					$scope.emptyPredictions();
				} else {
					// var promise = debouncedPredict(newQuery);
					$scope.preventNewPredictionsFromBeingPlacedOnScope = false;
					debouncedPredict(newQuery);
				}
			};

			// get new predictions when the query changes
			$scope.$watch('query', watchQueryToUpdatePredictions);

			// put predictions on the parent scope if requested
			if ($scope.options.placeOnParentScope) {
				$scope.$watch('predictions', function() {
					$scope.$parent.predictions = $scope.predictions;
				})
			}

			// change the query to the specified one
			// useful for when the user selects a prediction--to change the query to the prediction
			$scope.completeAs = function(newQuery) {
				$scope.query = newQuery;
			};

			// empty the prediction list in a way that resepcts the fragments option
			$scope.emptyPredictions = function() {
				if ($scope.options.fragments) {
					if (!$scope.predictions) {
						$scope.predictions = {};
					}
					$scope.predictions.predictions = [];
					$scope.predictions.query = $scope.query;
					$scope.predictions.resultsFound = 0;
				} else {
					$scope.predictions = [];
				}
			};

			// stitch several fragments into a complete predicted query (e.g. 'can', 'cer' fragments => 'cancer')
			$scope.stitchFragments = function(fragmentSet) {
				var stitchedQuery = "";
				for (var i = 0; i < fragmentSet.length; i++) {
					stitchedQuery += fragmentSet[i].fragment;
				}
				return stitchedQuery;
			};
		},
		// require: 'ngModel', // Array = multiple requires, ? = optional, ^ = check parent elements
		restrict: 'A', // E = Element, A = Attribute, C = Class, M = Comment
		// template: '',
		// templateUrl: '',
		// replace: true,
		transclude: true,
		// compile: function(tElement, tAttrs, function transclude(function(scope, cloneLinkingFn){ return function linking(scope, elm, attrs){}})),
		link: function($scope, $element, $attrs, controller, transclude) {
			// allow the transcluded HTML to use this directive's isolate scope
			// see http://angular-tips.com/blog/2014/03/transclusion-and-scopes/
			transclude($scope, function(clone, scope) {
				$element.append(clone);
			});

			// create an empty prediction list initially
			$scope.emptyPredictions();
		}
	};
}]);

'use strict';

angular.module('dukemedicine')
    .directive('ngRelatedArticle', function () {
        return {
            restrict: 'A',
            scope: {
                item: '='
            },
            template:
                '<a href="{{item.url}}" class="related-article-image-link">' +
                    '<img class="related-article-preview-image" src="{{imageUrl}}" width="{{item.preview_image.width}}" height="{{item.preview_image.height}}" alt="{{item.preview_image.title}}">' +
                '</a>' +
                '<div class="related-article-title">' +
                    '<a href="{{item.url}}" class="related-article-description-link">{{item.title}}</a>' +
                '</div>',
            controller: function($scope, FileService) {//TODO: Set preview image to a default if undefined

                $scope.getFid = function(item){
                    if(item.field_preview_image.und !== undefined && item.field_preview_image.und !== null){
                        return item.field_preview_image.und[0].fid;
                    }

                };

                FileService.getFile($scope.getFid($scope.item), 'home_page_spotlight')
                    .then(function(data) {
                        if(data.error === undefined) {
                            $scope.imageUrl = data.image_url ? data.image_url : '';
                        } else {
                            $scope.imageUrl = '/sites/www.dukemedicine.org/themes/dukezen/images/blog_feed_no_image.png';
                            console.log(data.error);
                        }
                    }, function(errorData) {
                        console.log(errorData.error);
                    });
            }
        };
    });

'use strict';

angular.module('dukemedicine').directive('ngSimpleSuggest', function () {
	return {
		restrict: 'A',
		transclude: true,
		template:
			'<form data-ng-submit="submitQuery()" class="input-prepend">' +
				'<input class="ajax-search-box" type="text" placeholder="{{placeholderText}}" data-ng-model="query">' +
				'<button type="submit" class="add-on"><i class="icon-search"></i></button>' +
			'</form>'+
			'<div id="background" data-ng-hide="query === \'\'"></div>'+
			'<ul id="auto-complete-result" class="simple-suggest" data-ng-class="{hideme: queryEmpty()}">'+
				'<li data-ng-if="doctorPhoto" data-ng-repeat="result in results" class="slide-in" data-ng-class="{last: $last}">'+
					'<a class="result-title" data-ng-href="{{result.ss_url | encodeResultUrl}}" data-ng-click="closeModal($event)"></a>'+
					'<i data-ng-class="getIcon(result)" data-ng-if="!noIcon"></i>'+
					'<div data-ng-if="doctorPhoto" data-ng-doctor-small-thumbnail class="doctor-photo-holder" data-doctor="result"></div>'+
					'<div class="result-info">'+
						'<span class="result-title">{{result.tm_title[0]}}</span>'+
						'<p data-ng-if="hasSpecialties(result)">'+
							'<span data-ng-repeat="specialty in result.sm_field_specialties">{{specialty}}{{$last ? \'\' : \', \'}}</span>'+
						'</p>'+
						'<p data-ng-if="hasLocation(result)">'+
							'<span class="location">{{result.tm_field_location_address$city[0]}}, {{result.tm_field_location_address$province[0]}}</span>'+
						'</p>'+
					'</div>'+
				'</li>'+
				'<li data-ng-if="!doctorPhoto" data-ng-repeat="result in results" class="slide-in" data-ng-class="{last: $last}">'+
					'<a class="result-title" data-ng-href="{{result.ss_url | encodeResultUrl}}" data-ng-click="closeModal($event)"></a>'+
					'<i data-ng-class="getIcon(result)" data-ng-if="!noIcon"></i>'+
					'<div data-ng-if="doctorPhoto" data-ng-doctor-small-thumbnail class="doctor-photo-holder" data-doctor="result"></div>'+
					'<div class="result-info">'+
						'<span class="result-title">{{result.tm_title[0]}}</span>'+
						'<p data-ng-if="hasSpecialties(result)">'+
							'<span data-ng-repeat="specialty in result.sm_field_specialties">{{specialty}}{{$last ? \'\' : \', \'}}</span>'+
						'</p>'+
						'<p data-ng-if="hasLocation(result)">'+
							'<span class="location">{{result.tm_field_location_address$city[0]}}, {{result.tm_field_location_address$province[0]}}</span>'+
						'</p>'+
					'</div>'+
				'</li>'+
			'</ul>'+
			'<div data-ng-transclude class="quick-transclude" data-ng-hide="hideTransclude"></div>',
		scope: {
			searchType: '=',
            isModalSearch: '='
		},
		controller: function($scope, $location, Debounce, $rootScope, SolrService, AllSearchParams, DoctorSearchParams, TreatmentSearchParams, LocationSearchParams) {
			$scope.location = $location;
			$scope.query = '';
			$scope.solr = SolrService;
			$scope.results = {};
			$scope.hideTransclude = false;
			$scope.debounce = Debounce;

			$scope.allSearch = AllSearchParams;
			$scope.doctorSearch = DoctorSearchParams;
			$scope.treatmentSearch = TreatmentSearchParams;
			$scope.locationSearch = LocationSearchParams;

			$scope.count = 8;
			$scope.params = [];
			$scope.qf = [];
			$scope.pf = [];
			$scope.fq = [];
			$scope.returnFields = [];


			$scope.queryEmpty = function() {
				return $scope.query === '';
			};

			$scope.getIcon = function(result) {
				if(result !== undefined && (result.ss_type !== undefined || result.ss_type !== '')){
					if(result.ss_type === 'blog' || result.ss_type === 'blog_post'){
						return 'icon-quote-right';
					}

					if(result.ss_type === 'events'){
						return 'icon-calendar';
					}

					if(result.ss_type === 'patients_and_visitors' || result.ss_type === 'patient_story' || result.ss_type === 'general_page' || result.ss_type === 'super_location_detail' || result.ss_type === 'patient_experience'){
						return 'icon-group';
					}

					if(result.ss_type === 'treatment_detail' || result.ss_type === 'treatment_group'){
						return 'icon-plus-sign-alt';
					}

					if(result.ss_type === 'staff_member' || result.ss_type === 'physician_profile'){
						return 'icon-user-md';
					}

					if(result.ss_type === 'hospital_location' || result.ss_type === 'treatment_location' || result.ss_type === 'super_location_page'){
						return 'icon-map-marker';
					}
				}

				return '';
			};

			$scope.getTypeName = function(result){
				if(result !== undefined && (result.ss_type !== undefined || result.ss_type !== '')){
					if(result.ss_type === 'blog' || result.ss_type === 'blog_post'){
						return 'Blog';
					}

					if(result.ss_type === 'events'){
						return 'Events';
					}

					if(result.ss_type === 'patients_and_visitors' || result.ss_type === 'patient_story' || result.ss_type === 'general_page' || result.ss_type === 'super_location_detail' || result.ss_type === 'patient_experience'){
						return 'General';
					}

					if(result.ss_type === 'treatment_detail' || result.ss_type === 'treatment_group'){
						return 'Treatment';
					}

					if(result.ss_type === 'staff_member' || result.ss_type === 'physician_profile'){
						return 'Doctor';
					}

					if(result.ss_type === 'hospital_location' || result.ss_type === 'treatment_location' || result.ss_type === 'super_location_page'){
						return 'Location';
					}
				}

				return '';
			};

			$scope.hasSpecialties = function(result) {
				if(result.sm_field_specialties !== undefined &&
					result.sm_field_specialties.length > 0){
						return true;
					}
					return false;
				};

			$scope.hasLocation = function(result) {
				if(result.ss_type === 'hospital_location' || result.ss_type === 'treatment_location' || result.ss_type === 'super_location_page'){
					return true;
				}
				return false;
			};

			$scope.setDataType = function(dataType){
				switch(dataType){
					case 'doctors':
						$scope.fq = $scope.doctorSearch.getFilterQueries();
						$scope.pf = $scope.doctorSearch.getPhraseFields();
						$scope.qf = $scope.doctorSearch.getQueryFields();
						$scope.params = $scope.doctorSearch.getParams(0, 0, $scope.count);
						$scope.returnFields = $scope.doctorSearch.getReturnFields();
						// $scope.returnFields.push('ss_type');
						break;
					case 'treatments':
						$scope.fq = $scope.treatmentSearch.getFilterQueries();
						$scope.pf = $scope.treatmentSearch.getPhraseFields();
						$scope.qf = $scope.treatmentSearch.getQueryFields();
						$scope.params = $scope.treatmentSearch.getParams(0, $scope.count);
						break;
					case 'locations':
						$scope.fq = $scope.locationSearch.getFilterQueries();
						$scope.pf = $scope.locationSearch.getPhraseFields();
						$scope.qf = $scope.locationSearch.getQueryFields();
						$scope.params = $scope.locationSearch.getParams(0, 0, $scope.count);
						break;
					default:
						$scope.fq = $scope.allSearch.getFilterQueries();
						$scope.pf = $scope.allSearch.getPhraseFields();
						$scope.qf = $scope.allSearch.getQueryFields();
						$scope.params = $scope.allSearch.getParams(0, $scope.count);
						break;
				}
			};

			$scope.submitQuery = function() {
				var query = '';
				if ($scope.query !== undefined){
					query = $scope.query;
				}

				window.location = encodeURI($scope.url + query);

				// If a modal menu i.e. main menu lightbox is open,
				// ensure it closes.
				if(!!$scope.$parent.closeModal){
					$scope.$parent.closeModal();
				}
			};

			$scope.debouncedQuery = function() {
				$scope.solr.customQuery('solr.autoSuggest', $scope.query, $scope.fq, $scope.pf, $scope.qf, [], $scope.params, $scope.returnFields);
			};


			$scope.makeQueryDebounce = $scope.debounce($scope.debouncedQuery, 425, false);

			$scope.$watch('query', function(newValue, oldValue){
				$scope.query = newValue;
				if(!!newValue && newValue !== oldValue){
					$scope.makeQueryDebounce(newValue);
					if($scope.hasTransclude){
						$scope.hideTransclude = true;
					}
				} else if(newValue !== undefined && newValue === ''){
					if($scope.hasTransclude) {
						$scope.hideTransclude = false;
					}
					$scope.results = [];
				}
			});

			$scope.$on('solr.autoSuggest', function(sender, newResults){
				if($scope.query === ''){
					$scope.results = [];
				} else{
					$scope.results = newResults.response.docs;
				}

			});

            $scope.isOnSameNgApp = function($event) {
                var targetArray = $event.target.getAttribute('data-ng-href').split('/');
                var pathArray = window.location.pathname.split('/');

                return targetArray.length > 0 && pathArray.length > 0 && targetArray[1] === pathArray[1];
            };

            // This function needs to called from data-ng-click on an achor tag
            // if the user is on angular page of same type that they select in the results.
            // example: If the user is on a doctor profile page and they select
            // another doctor profile page in the list of suggestions.
            // To ignore this behavior, add an attribute called data-is-modal-search="false" where
            // the directive is used in a view.
            // We are currently ignoring this behavior for the search box on the find-a-doc page.
            $scope.closeModal = function($event) {
                if ($scope.isModalSearch && $event) {
                    if ($scope.isOnSameNgApp($event)) {
                        window.location = $event.target.href;
                        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
                            $scope.$parent.toggleMenu('');
                        } else {
                            window.location.reload(false);
                        }
                    }
                }

            };
		},
		link: function(scope, element, attrs, controller, transcludeFn) {
			if (!!attrs.url) {
				scope.url = attrs.url;
			}

			if( !!attrs.count){
				scope.count = parseInt(attrs.count);
			}

			if (!!attrs.ignorePath) {
				scope.ignorePath = attrs.ignorePath;
			}

			if (!!attrs.placeholderText) {
				scope.placeholderText = attrs.placeholderText;
			}

			if (!!attrs.searchType) {
				scope.setDataType(attrs.searchType);
			}

			if (attrs.noIcon !== undefined) {
				scope.noIcon = true;
			}

			if (attrs.doctorPhoto !== undefined) {
				scope.doctorPhoto = true;
			}

			element.bind('submit', function() {
				element.find('input').blur();
			});

			var thisScope = scope;
			transcludeFn(function(clone){
				if(clone.length > 1){
					thisScope.hasTransclude = true;
				} else{
					thisScope.hasTransclude = false;
				}
			});
		}
	};
});

'use strict';

angular.module('dukemedicine')
  .directive('ngSolrFullPagination', function () {
    return {
      restrict: 'A',
      scope: {
          start: '=',
          count: '=',
          rows: '='
      },
      template:'<a data-ng-click="prev()" data-ng-hide="!hasPrev" class="pager-prev button white-button">Previous</a>'+
                  '<div id="pagination-wrapper" data-ng-hide="!hasPrev && !hasNext" data-ng-class="{\'noNext\': !hasNext , \'noPrev\': !hasPrev}">'+
				    '<div class="table-center">'+
	                    '<ul id="pagination-list">'+
	                      '<li data-ng-class="{\'active\': isActive(1)}"><a data-ng-click="setPage(1)">1</a></li>'+
	                      '<li class="ellipsis" data-ng-show="needsFrontEllipsis">...</li>'+
	                      '<li data-ng-repeat="page in getMiddlePages(rows, count)" data-ng-class="{\'active\': isActive(page)}">'+
	                        '<a data-ng-click="setPage({{page}})">{{page}}</a>'+
	                      '</li>'+
	                      '<li class="ellipsis" data-ng-show="needsEndEllipsis">...</li>'+
	                      '<li class="last" data-ng-class="{\'active\': isActive(totalPages(rows, count))}" data-ng-hide="totalPages(rows, count) === 1">'+
	                        '<a data-ng-click="setPage(totalPages(rows, count))">{{totalPages(rows, count)}}</a>'+
	                      '</li>'+
	                    '</ul>'+
					'</div>'+
                  '</div>'+
                '<a data-ng-click="next()" data-ng-hide="!hasNext" class="pager-next button white-button">Next</a>',
      controller: function($scope, SolrService, $location, $anchorScroll) {

          $scope.location = $location;
          $scope.needsEndEllipsis = false;
          $scope.needsFrontEllipsis = false;

          $scope.updatePageAndScrollToTop = function(pageNumber) {
              $scope.location.search('page', pageNumber);
              var oldHash = $location.hash();
              $scope.location.hash('top-of-result');
              $anchorScroll();
              $scope.location.hash(oldHash);
          };

          $scope.prev = function() {
              var searchParams = SolrService.getParams(),
                  newStart = (searchParams.start !== undefined) ? (searchParams.start - parseInt($scope.$parent.rows))/parseInt($scope.$parent.rows) : $scope.start;

              $scope.updatePageAndScrollToTop(newStart);
          };

          /*Calls solr service with the next start value */
          $scope.next = function() {
              var searchParams = SolrService.getParams(),
                  newStart = (searchParams.start !== undefined) ? (searchParams.start + parseInt($scope.rows))/parseInt($scope.rows) : $scope.start;

              $scope.updatePageAndScrollToTop(newStart);
          };

          /* Returns if there is a previous page or not */
          $scope.checkNextAndPrev = function(start, rows, count) {
              if(start === 0 || start - rows < 0){
                  $scope.hasPrev = false;
              } else{
                  $scope.hasPrev = true;
              }

              if(start + rows >= count){
                  $scope.hasNext = false;
              } else{
                  $scope.hasNext = true;
              }
          };

          $scope.getCurrentPage = function(){
            if($location.$$search.page !== undefined){
              return parseInt($scope.location.$$search.page) + 1;
            }

            return 1;
          };

          $scope.getMiddlePages = function(rows, count){
              var totalPages =  $scope.totalPages(rows, count),
                visible = [],
                currentPage = $scope.getCurrentPage(),
                startMiddle = 0;

              if(totalPages > 2){

                  if(totalPages > 5){
                      if(currentPage === 1 || currentPage < 4){
                          startMiddle = 2;
                          $scope.needsFrontEllipsis = false;
                          $scope.needsEndEllipsis = true;
                      } else if(currentPage === 4 && (currentPage + 2) !== totalPages){
                          startMiddle = 3;
                          $scope.needsFrontEllipsis = true;
                          $scope.needsEndEllipsis = true;
                      } else if(currentPage === totalPages || currentPage > (totalPages - 2)){
                          startMiddle = totalPages - 3;
                          $scope.needsEndEllipsis = false;
                          $scope.needsFrontEllipsis = true;
                      } else if(currentPage === (totalPages - 3)){
                          startMiddle = totalPages - 4;
                          $scope.needsFrontEllipsis = true;
                          $scope.needsEndEllipsis = true;
                      } else{
                          startMiddle = $scope.getCurrentPage() - 1;
                          $scope.needsEndEllipsis = true;
                          $scope.needsFrontEllipsis = true;
                      }
                  } else{
                    startMiddle = 2;
					$scope.needsEndEllipsis = false;
					$scope.needsFrontEllipsis = false;
                  }

                  for(var i = 0; i < Math.min(3, totalPages - 2); i++){
                      visible.push(startMiddle + i);
                  }
              } else{
                  $scope.needsEndEllipsis = false;
                  $scope.needsFrontEllipsis = false;
              }


              return visible;
          };

          $scope.isActive = function(pageNumber){
              return pageNumber === $scope.getCurrentPage();
          };

          $scope.setPage = function(pageNumber){
              $scope.updatePageAndScrollToTop(pageNumber - 1);
          };

          $scope.totalPages = function(rows, count) {
              if (rows !== undefined && count !== undefined && rows > 0) {
                  return count <= rows ? 1 : Math.ceil(count/rows);
              } else {
                  return 1;
              }
          };

      },
      link: function($scope, element, attrs) {
          $scope.hasPrev = true;
          $scope.hasNext = true;

          $scope.$watch(attrs.start, function(newVal, oldVal) {
              if(newVal !== oldVal){
                  $scope.start = newVal;
                  $scope.checkNextAndPrev(parseInt($scope.start), parseInt($scope.rows), parseInt($scope.count));
              }
          });

          $scope.$watch(attrs.rows, function(newVal, oldVal) {
              if(newVal !== oldVal){
                  $scope.rows = newVal;
              }
          });

          $scope.$watch(attrs.count, function(newVal, oldVal) {
              if(newVal !== oldVal){
                  $scope.count = newVal;
                  $scope.checkNextAndPrev(parseInt($scope.start), parseInt($scope.rows), parseInt($scope.count));
              }
          });
      }
    };
  });

'use strict';

angular.module('dukemedicine')
    .directive('mySpotlightContent', function () {
        return {
            restrict: 'A',
            scope: {
                item: '='
            },
            template:
                '<a href="{{item.link}}" class="image-link">' +
                    '<img src="{{imageUrl}}" alt="{{item.imageAltText}}" typeof="foaf:Imaage" width="{{item.imageWidth}}" height="imageHeight">' +
                '</a>' +
                '<span class="more-link-text">' +
                    '<a href="{{item.link}}" class="description-link" target="{{item.target}}">{{item.title}}</a>' +
                    '<span class="link-type"><i class="{{icon}}"></i>{{linkType}}</span>' +
                '</span>',

            controller: function($scope, FileService) {
                FileService.getFile($scope.item.imageId, 'more_from_duke')
                    .then(function(data) {
                        if(data.error === undefined) {
                            $scope.imageUrl = data.image_url ? data.image_url : '';
                        } else {
                            console.log(data.error);
                        }
                    }, function(errorData) {
                        console.log(errorData.error);
                    });
            },
            link: function(scope){
                switch (scope.item.type) {
                case 'blog':
                    scope.linkType = 'Blog';
                    scope.icon = 'icon-quote-left';
                    break;
                case 'blog_post':
                    scope.linkType = 'Blog';
                    scope.icon = 'icon-quote-left';
                    break;
                case 'news':
                    scope.linkType = 'News';
                    scope.icon = 'icon-exclamation-sign';
                    break;
                case 'clinical trial':
                    scope.linkType = 'Clinical trials';
                    scope.icon = 'icon-beaker';
                    break;
                default:
                    scope.linkType = '';
                    scope.icon = '';
                    break;
                }
            }
        };
    });

'use strict';

angular.module('dukemedicine').directive('ngToggleMap', ['$animate', '$window', function($animate, $window){
    return function($scope, element, attrs) {
        $scope.$watch(attrs.ngToggleMap, function(newVal) {
            if($window.innerWidth < 767){
                if(newVal) {
                    $animate.addClass(element, 'slideLeftOutMobile');
                } else {
                    $animate.removeClass(element, 'slideLeftOutMobile');
                }
            } else{
                if(newVal) {
                    $animate.addClass(element, 'slideLeftOut');
                } else {
                    $animate.removeClass(element, 'slideLeftOut');
                }
            }
        });
    };
}]);
'use strict';

angular.module('dukemedicine').directive('ngTogglePanel', ['$animate', '$window', function($animate, $window){
    return function($scope, element, attrs) {
        $scope.$watch(attrs.ngTogglePanel, function(newVal) {
            if($window.innerWidth < 767){
                if(newVal) {
                    $animate.addClass(element, 'slideLeftRemoveMobile');
                } else {
                    $animate.removeClass(element, 'slideLeftRemoveMobile');
                }
            } else{
                if(newVal) {
                    $animate.addClass(element, 'slideLeftRemove');
                } else {
                    $animate.removeClass(element, 'slideLeftRemove');
                }
            }

        });
    };
}]);
'use strict';

angular.module('dukemedicine')
    .directive('ngResizeClear', ['$window', function($window){
        return {
            restrict: 'A',
            scope: {},
            controller: function($scope, $window){
                $scope.window = $window;
                $scope.hasParentAlert = $scope.$parent.updateWindowResize !== undefined ? true : false;
                $scope.clearInlineStyles = function(e){
                    angular.element(e).removeAttr('style');
                };

                $scope.alertParentGrow = function(){
                    if($scope.hasParentAlert){
                        $scope.$parent.updateWindowResize('grow');
                    }
                };

                $scope.alertParentShrink = function(){
                    if($scope.hasParentAlert){
                        $scope.$parent.updateWindowResize('shrink');
                    }
                };
            },
            link: function(scope, element){
                scope.windowWidth = $window.outerWidth;
                angular.element(scope.window).bind('resize',function(){
                    if(scope.windowWidth !== scope.window.outerWidth && ((scope.window.outerWidth > 766 && scope.windowWidth <= 766) || (scope.window.outerWidth < 766 && scope.windowWidth >= 766))){
                        scope.clearInlineStyles(element);

                        if(scope.window.outerWidth > 766){
                            scope.alertParentGrow();
                        } else{
                            scope.alertParentShrink();
                        }
                    }
                    scope.windowWidth = scope.window.outerWidth;
                });
            }
        };
    }]);

'use strict';

angular.module('dukemedicine.system')
.directive('ngYoutubeVideo', function () {
  return{
      restrict: 'A',
      scope: {
        url: '='
      },
      template:
        '<div class="media-youtube-video">' +
        '   <iframe src="{{doctorYoutubeVideo}}" frameborder="0" allowfullscreen></iframe>' +
        '</div>',
      controller: function($scope){

        $scope.getYoutubeVideoId = function(youtubeUrl){
            // Ultimately we just need the ID for the youtube video.
            // Since there are always several urls for a video, we try to determine
            // which url was entered and extract the video ID.
            if (youtubeUrl.indexOf('v=') > -1 && youtubeUrl.indexOf('feature') > -1) {
                // The 'share' url from the youtube site was pasted into the browser and got redirected.
                // The user then copied the redirect url from the browser.
                // example: https://www.youtube.com/watch?v=1w9N1BaonfY&feature=youtu.be
                return youtubeUrl.split('&').shift().split('=').pop();
            } else if (youtubeUrl.indexOf('v=') > -1 && youtubeUrl.indexOf('feature') === -1) {
                // The user went the youtube site and copied the url from the browser.
                // example: https://www.youtube.com/watch?v=1w9N1BaonfY
                return youtubeUrl.split('v=').pop();
            } else {
                // The user used the 'share' url from the youtube site
                // example: http://youtu.be/1w9N1BaonfY
                // OR
                // The user used the embed url from the youtube site.
                // example:  http://www.youtube.com/embed/1w9N1BaonfY
                return youtubeUrl.split('/').pop();
            }
        };
        $scope.doctorYoutubeVideo = 'https://www.youtube.com/embed/' + $scope.getYoutubeVideoId($scope.url);
      }
    };

});

'use strict';

angular.module('dukemedicine')
    .filter('encodeHtml', function () {
        var entities= {
          '&amp;': '&',
          '&lt;': '<',
          '&gt;': '>',

          '&quot;': '"',
          '&QUOT;': '"',
          '&#x00022;': '"',
          '&#34;': '"',

          '&#039;': '\'',
          '&#x00027;': '\'',
          '&apos;': '\''
        };

        return function(text) {
            for (var prop in entities) {
              if (entities.hasOwnProperty(prop)) {
                text = text.replace(new RegExp(prop, 'g'), entities[prop]);
              }
            }

            return text;
        };
    });
'use strict';

angular.module('dukemedicine')
    .filter('encodeResultUrl', function () {
        return function(text) {
            if(!!text) {
                var reg = /.+?\:\/\/.+?(\/.+?)(\?|$)/;
                return reg.exec(text)[1];
            }

            return '';
        };
    });

'use strict';

angular.module('dukemedicine')
    .filter('getFullNameAndDegrees', function () {
        return function(text) {
            var result = '';
            if (text !== undefined){
                if (text.ss_field_physician_first_name !== undefined){
                    var firstName = text.ss_field_physician_first_name;
                    result += firstName.charAt(0).toUpperCase() + firstName.slice(1);
                }

                if (text.ss_field_physician_middle_name !== undefined){
                    var middleName = text.ss_field_physician_middle_name;
                    result += ' ' + middleName.charAt(0).toUpperCase() + middleName.slice(1);
                }

                if (text.ss_field_physician_last_name !== undefined){
                    var lastName = text.ss_field_physician_last_name;
                    lastName = lastName.charAt(0).toUpperCase() + lastName.slice(1);

                    // check for names with an apostrophe e.g. O'Malley
                    var pos = lastName.indexOf('\'');
                    if (pos > -1){
                        lastName = lastName.slice(0, pos + 1) + lastName.charAt(pos + 1).toUpperCase() + lastName.slice(pos + 2);
                    }

                    // check for names with Mc e.g. McNally
                    pos = lastName.indexOf('Mc');
                    if (pos > -1){
                        lastName = lastName.slice(0, pos + 2) + lastName.charAt(pos + 2).toUpperCase() + lastName.slice(pos + 3);
                    }

                    result += ' ' + lastName;
                }


                if (text.sm_field_physician_degrees !== undefined){
                    result += ', ' + text.sm_field_physician_degrees.join(', ');
                }
            }
            return result;
        };
    });

'use strict';

angular.module('dukemedicine')
    .filter('htmlToPlainText', function () {
        return function(text) {
            return String(text).replace(/<[^>]+>/gm, '');
        };
    });
'use strict';
/**
* THIS DOES NOT ACCOUNT FOR EDGE CASES OR STRINGS THAT ARE MULTIPLE SENTENCES.
**/
angular.module('dukemedicine')
.filter('sentencecase', function () {
  return function(text) {
    //TODO: loop over @param:text to search for puncutations (. ! ?)
    // and construct string that handles multiple sentences.

    var result = '';
    result = text.charAt(0).toUpperCase() + text.slice(1);
    return result;
  };
});

'use strict';

angular.module('dukemedicine')
    .filter('truncate', function () {
        return function (text, length, end) {
            if (isNaN(length))
                length = 10;

            if (end === undefined)
                end = ' ...';

            if (text.length <= length || text.length - end.length <= length) {
                return text;
            }
            else {
              text = text.substring(0, length);
              text = text.substring(0, text.lastIndexOf(' '));
              return String(text) + end;
            }

        };
    });

'use strict';

angular.module('dukemedicine')
    .filter('truncateBracket', function () {
        return function (text, length, end) {
            if (isNaN(length))
                length = 10;

            if (end === undefined)
                end = ' [...]';

            if (text.length <= length || text.length - end.length <= length) {
                return text;
            }
            else {
              text = text.substring(0, length);
              text = text.substring(0, text.lastIndexOf(' '));
              return String(text) + end;
            }

        };
    });

'use strict';


