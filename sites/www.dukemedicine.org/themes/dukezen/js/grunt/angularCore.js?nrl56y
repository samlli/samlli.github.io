angular.module('dukemedicine').animation('.slideLeftRemove', function() {
    return {
        // Expand to the whole width
        addClass: function(element, className, done) {
            TweenMax.to(element, 0.35, {
                left: '-35%',
                opacity: 0,
                ease: 'Elastic.easOut',
                onComplete: function(){
                    angular.element(element).scope().map.refresh();
                    done();
                }
            });
        },
        // Restore width
        removeClass: function(element, className, done) {
            TweenMax.to(element, 0.35, {
                left: '0%',
                opacity: 1,
                ease: 'Elastic.easOut',
                onComplete: function(){
                    angular.element(element).scope().map.refresh();
                    done();
                }
            });
        }
    };
});

