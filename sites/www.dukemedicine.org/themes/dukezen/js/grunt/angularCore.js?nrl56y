angular.module('dukemedicine')
	.directive('autoFocus', ['$timeout', function($timeout) {
		return {
			restrict: 'A',
			link: function(scope, element) {
				$timeout(function(){
					element[0].focus();
				}, 250);
			}
		};
	}]);

'use strict';

angular.module('dukemedicine')
    .directive('myColumnizer', function () {
        return {
            restrict: 'A',
            scope: {
                result: '='
            },
            template:
                '<span class="header5 collapseable hidden">' +
                  '<i class="icon-minus-sign"></i>' +
                  '<span class="ellipsis" data-ng-bind-html="title"></span>' +
                '</span>' +
                '<div class="result-item" data-ng-bind-html="result.desc"></div>',

            link: function(scope, element) {
                scope.title = scope.result.title;

                if (scope.result.hasUrl){
                    var isExternal = (scope.result.url.indexOf('http://') > -1) ||
                        (scope.result.url.indexOf('https://') > -1);

                    var target = '';
                    if (isExternal){
                        target = ' target="_blank"';
                    }

                    scope.title = '<a href="' + scope.result.url + '"' + target + '>' +
                        scope.result.title + '</a>';
                }

                // Run jquery plugin columnizer AFTER all elements are added to the DOM.
                if (scope.$parent.$last){
                    element.parent().makeacolumnlists({cols:2,colWidth:0,equalHeight:false,startN:1});
                }

                // Bind click event to collapsed items in mobile view.
                element.find('.collapseable').on('click', function(){
                    element.toggleClass('hidden');
                });
            }
        };
    });

'use strict';

angular.module('dukemedicine')
.directive('ngDoctorRating', ['FetchDoctorRatingService','FetchConfigVariableService', function(FetchDoctorRatingService,FetchConfigVariableService) {
//.directive('ngDoctorRating', function() {
    var linker = function (scope, element, attrs) {

      var doctorId = attrs["ngDoctorRating"];
      scope.stars = 5;
      scope.reviewcount;

      scope.is_doctor_ratings_active = false;
      FetchConfigVariableService.getConfigVariable('is_doctor_ratings_active').then(function(configVariable){
        scope.is_doctor_ratings_active = configVariable.value;
        if(scope.is_doctor_ratings_active == true){
          FetchDoctorRatingService.fetchDoctorRatingBySlug(doctorId).then(function(rating) {
            scope.stars = rating.profile.averageStarRatingStr;
            scope.reviewcount = rating.profile.reviewcount;
          });
        }
        else
          scope.is_doctor_ratings_active = false;
      })
      .catch(function(error) {
           scope.is_doctor_ratings_active = false;
        });


    };
    var controller = function ($scope) {
      // Pending
      //https://www.docscores.com/widget/api/org-profile/demo-health/slug/stephen-wright/10


    };
    return {
      restrict: 'A',
      controller: controller,
      controllerAs: 'doctorRating',
      link: linker,
      scope: {
          text: "=doctorId"
        },
      template:
    		'<span ng-show="is_doctor_ratings_active">'+
        '<i class="icon-star-empty empty-star" ng-if="stars<=0" style="font-family: FontAwesome;font-size: 22px;color: #AAAAAA;line-height: 22px; margin-right: 10px;" ></i> '+
        '<i class="icon-star-half solid-star" ng-if="(stars>0&&stars<=.5)" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 20px;" ></i>'+
        '<i class="icon-star solid-star" ng-if="stars>.5" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 10px;" ></i>'+
        '<i class="icon-star-empty empty-star" ng-if="stars<=1" style="font-family: FontAwesome;font-size: 22px;color: #AAAAAA;line-height: 22px; margin-right: 10px;" ></i> '+
        '<i class="icon-star-half solid-star" ng-if="(stars>1&&stars<=1.5)" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 20px;" ></i>'+
        '<i class="icon-star solid-star" ng-if="stars>1.5" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 10px;" ></i>'+
        '<i class="icon-star-empty empty-star" ng-if="stars<=2" style="font-family: FontAwesome;font-size: 22px;color: #AAAAAA;line-height: 22px; margin-right: 10px;" ></i> '+
        '<i class="icon-star-half solid-star" ng-if="(stars>2&&stars<=2.5)" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 20px;" ></i>'+
        '<i class="icon-star solid-star" ng-if="stars>2.5" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 10px;" ></i>'+
        '<i class="icon-star-empty empty-star" ng-if="stars<=3" style="font-family: FontAwesome;font-size: 22px;color: #AAAAAA;line-height: 22px; margin-right: 10px;" ></i> '+
        '<i class="icon-star-half solid-star" ng-if="(stars>3&&stars<=3.5)" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 20px;" ></i>'+
        '<i class="icon-star solid-star" ng-if="stars>3.5" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 10px;" ></i>'+
        '<i class="icon-star-empty empty-star" ng-if="stars<=4" style="font-family: FontAwesome;font-size: 22px;color: #AAAAAA;line-height: 22px; margin-right: 10px;" ></i> '+
        '<i class="icon-star-half solid-star" ng-if="(stars>4&&stars<=4.5)" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 20px;" ></i>'+
        '<i class="icon-star solid-star" ng-if="stars>4.5" style="font-family: FontAwesome;font-size: 22px;color: #FFB940;line-height: 22px; margin-right: 10px;" ></i>'+
        '<span class=rating-text style="font-family: Open Sans;font-size: 18px; font-weight: 300;color: #333333;line-height: 24px;"> {{stars}} out of 5 ({{reviewcount}} reviews)</span>'+
        '</span>',
    };
}]);


