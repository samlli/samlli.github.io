angular.module('dukemedicine')
  .factory('QueryService', ['$location', function($location){

      var getQuery = function(){
          return $location.$$search.s !== undefined ? $location.$$search.s : '';
      };

      return {
          getQuery: getQuery
      };
}]);

'use strict';

angular.module('dukemedicine')
.factory('SolrCompletionService', ['$http', '$q', function($http, $q){
	return function SolrCompletionService(options) {
		// the default options
		this.defaults = {
			url: '/solr/select', // http://ec2-54-83-45-131.compute-1.amazonaws.com:8080/solr/www.dukemedicine.org
			endpoint: '/suggest'
			// this is a drupal api to interface with solr
			// we customize the specific endpooint using params (see below)
		};

		// allows this.options to be (partially) updated
		this.reconfigure = function(options) {
			// override the defaults with the passed options
			if (options != undefined && options != {}) {
				this.options = angular.extend({}, this.options, options);
				// this.options = jQuery.extend({}, this.defaults, options);
			}
		}

		// copy all the defaults onto the options
		this.options = {};
		angular.copy(this.defaults, this.options);

		// update the options according to what was passed when instantiating
		this.reconfigure(options);

		// returns a promise that provides completions when resolved
		this.complete = function(query) {
			// build the prediction request
			var params = {
				q: query,
				'miscParams[endpoint]': this.options.endpoint,
				wt: 'json',
				source: 'angular',
				// indent: true
			};

			// execute the rest of the code in the promise
			var returnedPromise = $q.defer();

			var promise = $http.post(this.options.url, params);

			var successHandler = function(response) {
				// read the query out of the initial request parameters
				var query = response.config.data.q;
				var completions = [];

				// what if there are no completions for the given query?
				try{
					if (!response.data.spellcheck.suggestions.hasOwnProperty(query)) {
						// callback with empty completion set
						returnedPromise.reject('Empty completion set.');
						return;
					}
				}
				catch(e){
					console.log('error in solrCompoletionService.js');
					return;
				}


				// read the completions out of the response
				completions = response.data.spellcheck.suggestions[query].suggestion;

				// resolve with completion set
				returnedPromise.resolve(completions);
			};

			var errorHandler = function(response) {
				returnedPromise.reject(response);
			};

			promise.then(successHandler, errorHandler);

			return returnedPromise.promise;

		};
	};
}]);

'use strict';


angular.module('dukemedicine')
.factory('SolrService', ['$rootScope', '$http', '$location', '$q', 'googleAnalytics',
    function($rootScope, $http, $location, $q, googleAnalytics){
        var queryVal = '',


            // Solr Proxy URL
            solrUrl = $location.protocol() + '://' + $location.host() + '/solr/select',

            //fq's
            filterQueries = [],

            // pf's
            phraseFields = [],

            // qf's
            queryFields = [],

            // bq's
            boostQueries = [],

            facetFields = {},
            activeFacets = [],
            facetQueries = [],
            spatialParams = {},

            returnFields = [],
            miscParams = {},

            // user's latitude and longitude
            userLocation = {
                latitude: null,
                longitude: null
            };



        var clearAll = function(){
            filterQueries = [];
            phraseFields = [];
            queryFields = [];
            boostQueries = [];
            facetFields = {};
            facetQueries = [];
            spatialParams = {};
            returnFields = [];
            miscParams = {};
        };

        var query = function(newQuery){
            if(newQuery === undefined) {
                newQuery = queryVal;
            } else{
                queryVal = newQuery;
            }

            var queryParams = {
                source: 'angular',
                q: queryVal,
                fq: filterQueries.concat(activeFacets),
                pf: phraseFields,
                qf: queryFields,
                bq: boostQueries,
                facets: facetFields,
                facetQueries: facetQueries,
                spatialParams: spatialParams,
                fl: returnFields,
                miscParams: miscParams,
                userLocation: this.getUserLocation()
            };

            $http.post(
                solrUrl,
                queryParams
            )
            .success(function(data) {
                $rootScope.$broadcast('solr.newQuery', data);
                googleAnalytics.setPage(window.location.pathname + window.location.hash);
            })
            .error(function(data) {
                console.log('Error ' + data);
            });
        };

        var customQuery = function(listener, cQuery, cfilterQuery, cphraseFields, cqueryFields, cboostQueries, cmsicParams, creturnFields){

            var queryParams = {
                source: 'angular',
                q: cQuery,
                fq: cfilterQuery,
                pf: cphraseFields,
                qf: cqueryFields,
                bq: cboostQueries,
                fl: creturnFields,
                miscParams: cmsicParams
            };

            $http.post(
                solrUrl,
                queryParams
            )
            .success(function(data) {
                $rootScope.$broadcast(listener, data);
            })
            .error(function(data) {
                console.log('Error ' + data);
            });
        };

        var deferQuery = function(cQuery, cfilterQuery, cphraseFields, cqueryFields, cboostQueries, cmsicParams, creturnFields){
            var deferred = $q.defer(),
                queryParams = {
                    source: 'angular',
                    q: cQuery,
                    fq: cfilterQuery,
                    pf: cphraseFields,
                    qf: cqueryFields,
                    bq: cboostQueries,
                    fl: creturnFields,
                    miscParams: cmsicParams
                };

            $http.post(
                solrUrl,
                queryParams
            )
            .success(function(data) {
                deferred.resolve(data);
            })
            .error(function(data) {
                deferred.reject(data);
            });

            return deferred.promise;
        };

        /**
        * Format for queries should follow as such:
        * {queryName: [cQuery, cfilterQuery, cphraseFields, cqueryFields, cboostQueries, cmsicParams, creturnFields]}
        */
        var groupQuery = function(queries){
            var deferred = $q.defer();

            $http.post(solrUrl, {source: 'angular', qType: 'groupQuery', queries: queries})
            .success(function(data) {
                deferred.resolve(data);
            })
            .error(function(data) {
                deferred.reject(data);
            });

            return deferred.promise;
        };

        var getQuery = function(){
            return queryVal;
        };

        // set fq's
        var setFilterQueries = function(fields) {
            filterQueries = fields;
        };

        var getFilterQueries = function() {
            return filterQueries;
        };

        // set pf's
        var setPhraseFields = function(fields) {
            phraseFields = fields;
        };

        // set qf's
        var setQueryFields = function(fields) {
            queryFields = fields;
        };

        var setBoostQueries = function(fields) {
            boostQueries = fields;
        };

        // set the facets
        var setFacetFields = function(facets) {
            facetFields = facets;
        };

        // queries that generate the distance facet fields.
        var setFacetQueries = function(queries) {
            facetQueries = queries;
        };

        var setSpatialParams = function(params) {
            spatialParams = params;
        };

        var getSpatialParams = function() {
            return spatialParams;
        };

        var setReturnFields = function(fields) {
            returnFields = fields;
        };

        var setParams = function(params) {
            miscParams = params;
        };

        var getParams = function() {
            return miscParams;
        };

        var setUrlParams = function(params) {
            var tmpParams = null;
            // Set the q's
            if(params.s !== undefined) {
                queryVal = params.s;
            } else {
                queryVal = '';
            }

            // Set facets
            if(params.fq !== undefined) {
                activeFacets = params.fq;
            } else {
                activeFacets = [];
            }

            // Set up the page
            if(params.page !== undefined && params.page) {
                tmpParams = this.getParams();
                tmpParams.start = params.page * tmpParams.rows;
                this.setParams(tmpParams);
            } else{
                tmpParams = this.getParams();
                tmpParams.start = 0;
                this.setParams(tmpParams);
            }

        };

        var setUserLocation = function(latitude, longitude) {
            if (typeof latitude === 'number'){
                userLocation.latitude = latitude;
                createCookie('userLat', latitude, 1);
            }

            if (typeof longitude === 'number'){
                userLocation.longitude = longitude;
                createCookie('userLong', longitude, 1);
            }

            this.query();
        };

        var getUserLocation = function() {
            if (userLocation.latitude === null || typeof(userLocation.latitude) == "undefined") {
                var latitude = readCookie('userLat');
                if (latitude !== null) {
                    userLocation.latitude = latitude;
                }
            }

            if (userLocation.longitude === null || typeof(userLocation.longitude) == "undefined") {
                var longitude = readCookie('userLong');
                if (longitude !== null) {
                    userLocation.longitude = longitude;
                }
            }

            return userLocation;
        };

        var clearUserLocation = function() {
            userLocation.latitude = null;
            userLocation.longitude = null;
            eraseCookie('userLat');
            eraseCookie('userLong');
            this.query();
        };

        var updateUrlParams = function(params) {
            this.setUrlParams(params);
            this.query();
        };

        $rootScope.$on('$routeUpdate', function(e, ang) {
            var solr = ang.scope.$$childHead.solr;
            var location = null;
            if(e.currentScope.$$childHead.location !== undefined) {
                location = e.currentScope.$$childHead.location;
            } else if(ang.scope.$$childHead.location !== undefined) {
                location = ang.scope.$$childHead.location;
            }

            if(location.$$search.hasOwnProperty('s') && location.$$search.s === undefined){
                try{
                    delete location.$$search.s;
                } catch(error){
                    location.$$search.s = '';
                }

                location.search(location.$$search);
            } else if(location.$$search.hasOwnProperty('page') && location.$$search.page === undefined){
                try{
                    delete location.$$search.page;
                } catch(error){
                    location.$$search.page = '';
                }

                location.search(location.$$search);
            } else if (solr !== undefined) {
                solr.updateUrlParams(location.$$search);
            }

            if(angular.element('.lt-ie8').size() > 0){
                window.location.reload(true);
            }

        });

        /* Ask for location -- happening too soon
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(function (position) {

              setUserLocation(position.coords.latitude, position.coords.longitude);
            });
        }
        */

        return {
            clearAll: clearAll,
            query: query,
            getQuery: getQuery,
            customQuery: customQuery,
            setFilterQueries: setFilterQueries,
            getFilterQueries: getFilterQueries,
            setQueryFields: setQueryFields,
            setBoostQueries: setBoostQueries,
            setFacetFields: setFacetFields,
            setPhraseFields: setPhraseFields,
            setReturnFields: setReturnFields,
            setParams: setParams,
            setUrlParams: setUrlParams,
            getParams: getParams,
            setUserLocation: setUserLocation,
            getUserLocation: getUserLocation,
            clearUserLocation: clearUserLocation,
            setFacetQueries: setFacetQueries,
            setSpatialParams: setSpatialParams,
            getSpatialParams: getSpatialParams,
            updateUrlParams: updateUrlParams,
            deferQuery: deferQuery,
            groupQuery: groupQuery
        };
}]);

'use strict';


angular.module('dukemedicine')
.factory('TreatmentSearchParams', ['SolrService', function(SolrService){

    var getFilterQueries = function(){
        return [
        ];
    };

