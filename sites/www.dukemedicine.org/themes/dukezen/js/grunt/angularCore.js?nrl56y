					'<li><a data-ng-href="/node/{{nodeId}}/edit">Edit</a></li>'+
					'<li><a data-ng-href="/node/{{nodeId}}/convert">Convert</a></li>'+
					'<li><a data-ng-href="/node/{{nodeId}}/log">Log</a></li>'+
				'</ul>',
			controller: function($scope) {
				$scope.loggedIn = function() {
					return window.user;
				};
			}
		};
