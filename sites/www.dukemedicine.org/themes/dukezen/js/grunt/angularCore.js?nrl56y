angular.module('dukemedicine', ['ui.unique', 'ngCookies', 'ngResource', 'ngRoute', 'ngTouch', 'ngAnimate', 'angular-momentjs', 'dukemedicine.system']);

'use strict';

//Setting Location Mode
angular.module('dukemedicine').config(['$locationProvider',
    function($locationProvider) {
        $locationProvider.hashPrefix('!');
    }
]);

// Make AngularJS $http service behave like jQuery.ajax()
angular.module('dukemedicine').config(['$httpProvider', function($httpProvider){

  $httpProvider.defaults.headers.common['X-Requested-With'] = 'XMLHttpRequest';

  // Use x-www-form-urlencoded Content-Type
  $httpProvider.defaults.headers.post['Content-Type'] = 'application/x-www-form-urlencoded;charset=utf-8';

  // Override $http service's default transformRequest
  $httpProvider.defaults.transformRequest = [function(data)
  {
    /**
     * The workhorse; converts an object to x-www-form-urlencoded serialization.
     * @param {Object} obj
     * @return {String}
     */
    var param = function(obj)
    {
      var query = '';
      var name, value, fullSubName, subName, subValue, innerObj, i;

      for(name in obj)
      {
        value = obj[name];

        if(value instanceof Array)
        {
          for(i=0; i<value.length; ++i)
          {
            subValue = value[i];
            fullSubName = name + '[' + i + ']';
            innerObj = {};
            innerObj[fullSubName] = subValue;
            query += param(innerObj) + '&';
          }
        }
        else if(value instanceof Object)
        {
          for(subName in value)
          {
            subValue = value[subName];
            fullSubName = name + '[' + subName + ']';
            innerObj = {};
            innerObj[fullSubName] = subValue;
            query += param(innerObj) + '&';
          }
        }
        else if(value !== undefined && value !== null)
        {
          query += encodeURIComponent(name) + '=' + encodeURIComponent(value) + '&';
        }
      }

      return query.length ? query.substr(0, query.length - 1) : query;
    };

    return angular.isObject(data) && String(data) !== '[object File]' ? param(data) : data;
  }];
}]);

angular.module('dukemedicine').config(['$sceProvider', function($sceProvider){
    $sceProvider.enabled(false);
}]);

angular.module('dukemedicine').config(['$sceDelegateProvider', function($sceDelegateProvider){
    $sceDelegateProvider.resourceUrlWhitelist(['self', '**']);
}]);
angular.module('dukemedicine.system', ['dukemedicine']);
'use strict';

angular.module('dukemedicine')
    .animation('.section-collapsed', function() {
        return{
            // Collapse the section
            beforeAddClass: function(element, className, done) {
                if(className === 'ng-hide'){

                    if(element.attr('oldHeight') === undefined){
                        element.attr('oldHeight', element.css('height'));
                    }


                    TweenMax.to(element, 0.25, {
                        height: '0px',
                        borderColor: '#FFF',
                        ease: 'Elastic.easOut',
                        onComplete: done
                    });
                } else{
                    done();
                }
            },
            // Expand the section
            removeClass: function(element, className, done){
                if(className === 'ng-hide'){
                    var remove = function(){
                        element.css('height', 'auto');
                        done();
                    };

                    if(element.attr('oldHeight') !== undefined){
                        TweenMax.to(element, 0.25, {
                            height: element.attr('oldHeight'),
                            backgroundColor: 'white',
                            borderColor: '#CCC',
                            ease: 'Elastic.easOut',
                            onComplete: remove
                        });
                    } else{
                        var newHeight = element.css('height', 'auto').css('height');
                        element.css('height', '0px');
                        element.attr('oldHeight', newHeight);
                        TweenMax.to(element, 0.25, {
                            height: newHeight,
                            borderColor: '#CCC',
                            backgroundColor: 'white',
                            ease: 'Elastic.easOut',
                            onComplete: remove
                        });
                    }
                } else{
                    done();
                }
            }
        };
    });

'use strict';

angular.module('dukemedicine').animation('.slideLeftOut', function() {
    return {
        // Expand to the whole width
        addClass: function(element, className, done) {

            element.attr('oldWidth', element.css('width')).attr('oldLeft', element.css('left'));

            TweenMax.to(element, 0.35, {
                width: '100%',
                left: '0',
                ease: 'Elastic.easOut',
                onComplete: function(){
                    angular.element(element).scope().map.refresh();
                    done();
                }
            });
        },
        // Restore width
        removeClass: function(element, className, done) {
            TweenMax.to(element, 0.35, {
                width: element.attr('oldWidth'),
                left: element.attr('oldLeft'),
                ease: 'Elastic.easOut',
                onComplete: function(){
                    angular.element(element).scope().map.refresh();
                    done();
                }
            });
        }
    };
});

